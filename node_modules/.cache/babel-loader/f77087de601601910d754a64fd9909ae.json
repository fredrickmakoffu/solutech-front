CZ,WAAK,KAAL,CAAW,GAAX,CAAe,kBAAkB,KAAK,WAAtC,EAAmD,IAAnD,CAAwD,UAAC,QAAD,EAAc;AACpE,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACA,QAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B;AACA,QAAA,MAAI,CAAC,SAAL,GAAiB,MAAI,CAAC,QAAL,CAAc,eAAd,CAA8B,MAA/C;;AAEA,YAAG,MAAI,CAAC,SAAL,GAAiB,CAApB,EAAuB;AACrB,UAAA,MAAI,CAAC,OAAL,GAAe,MAAI,CAAC,QAAL,CAAc,eAAd,CAA8B,MAAI,CAAC,MAAL,GAAY,CAA1C,EAA6C,OAA5D;AACF;AACD,OARD,EAQG,KARH,CAQS,YAAM;AACb,QAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,UAAA,IAAI,EAAE;AAAP,SAAlB;AACD,OAVD;AAWF;AA5CO;AA9DI,CAAf","sourcesContent":["<template>\n    <div id=\"wrapper\">\n        <div class=\"body\">\n            <navbar class=\"navbar-border-bottom pt-0 pb-0\"></navbar>\n\n            <breadcrumbs :breadcrumbs_data=breadcrumbs_data class=\"ps-4 pe-4 mt-2\"></breadcrumbs>\n\n            <section class=\"container property bg-white ps-4 pe-4\">\n                <div class=\"row\">\n                    <div class=\"col-12\">\n                        <h1 class=\"text-dark fw-bold\">{{ property.name }}</h1>\n                        <p>{{ property.address }}</p>\n                    </div>\n\n                    <div class=\"col-md-8 mt-4\">\n                        <div class=\"card border-0 d-flex overflow-hidden\">\n                          <transition v-for=\"({image_url}, index) in property.property_images\" :key=\"index\" name=\"slide-fade\" class=\"d-flex\">\n                            <img v-if=\"screen == index+1\" :src=\"'http://localhost:8000/' + image_url\" alt=\"\" class=\"img-fluid rounded\">\n                          </transition>\n\n                          <font-awesome-icon class=\"text-dark fw-bold right-circle bg-white bg-gradient\" :icon=\"['fas', 'arrow-right']\" v-if=\"screen != no_images\" @click=\"nextScreen()\"></font-awesome-icon> \n\n                          <font-awesome-icon class=\"text-dark fw-bold left-circle bg-white bg-gradient\" :icon=\"['fas', 'arrow-left']\" v-if=\"screen != 1\" @click=\"previousScreen()\"></font-awesome-icon>\n                        </div>\n                        \n                        <div class=\"d-flex\">\n                          <figcaption class=\"ms-auto me-1 mt-2\">{{ caption }}</figcaption>\n                        </div>\n                        \n                        <div class=\"mt-4\">\n                          <h5 class=\"text-dark mb-1 mt-2 fw-bold\">Description</h5>\n                          <p class=\"mt-4\" style=\"font-size: 1.1rem;\">{{ property.description }}</p> \n\n                          <div class=\"row mt-4\">\n                            <div class=\"col-md-6 col-sm-6\">\n                              <p class=\"mt-2 mb-3\" style=\"font-size: 17px;\"> \n                                <font-awesome-icon class=\"me-2 text-success\" style=\"font-size: 14px\" :icon=\"['fas', 'location-arrow']\"></font-awesome-icon> \n                                <span class=\"fw-bold\">{{ property.address }} </span> \n                              </p>\n\n                              <p class=\"mt-2 mb-3\" style=\"font-size: 17px;\"> \n                                <font-awesome-icon class=\"me-3 text-success\" style=\"font-size: 14px\" :icon=\"['fas', 'dollar-sign']\"></font-awesome-icon> \n                                <span v-if=\"property.price\" class=\"fw-bold\">Ksh. {{ formatNumber(property.price) }} </span> \n                              </p>\n\n                              <p class=\"mt-2 mb-3\" style=\"font-size: 17px;\"> \n                                <font-awesome-icon class=\"me-2 text-success\" style=\"font-size: 14px\" :icon=\"['fas', 'building']\"></font-awesome-icon> \n                                <span class=\"text-capitalize fw-bold\">{{ property.type }} </span>\n                              </p>                              \n                            </div>\n\n                            <div class=\"col-md-6 col-sm-6\">\n                              <p class=\"mt-2 mb-3\" style=\"font-size: 17px;\"> \n                                <font-awesome-icon class=\"me-2 text-success\" style=\"font-size: 14px\" :icon=\"['fas', 'house-user']\"></font-awesome-icon> \n                                <span class=\"text-capitalize fw-bold\">{{ property.bedrooms }} Bedrooms</span>\n                              </p>\n\n                              <p class=\"mt-2 mb-3\" style=\"font-size: 17px;\"> \n                                <font-awesome-icon class=\"me-2 text-success\" style=\"font-size: 14px\" :icon=\"['fas', 'house-user']\"></font-awesome-icon> \n                                <span class=\"text-capitalize fw-bold\">{{ property.bathrooms }} bathrooms</span>\n                              </p>\n                            </div>                        \n                          </div>\n                        </div>                    \n\n                        <div class=\"mt-4\">\n                          <h5 class=\"text-dark mb-1 mt-2 fw-bold\">Amenities</h5>\n                          \n                          <div class=\"row\" >\n\n                            <div v-for=\"({amenity}, index) in property.amenities\" :key=\"index\" class=\"col-md-3 col-sm-6 mt-4\">\n                              <div class=\"card bg-light p-2 pt-3 pb-3\">\n                                <h4 class=\"text-center\">\n                                <font-awesome-icon :icon=\"['fas', amenity_icons[amenity]]\"></font-awesome-icon>\n                                </h4>\n                                <p class=\"text-center mb-0 mt-2\">{{ amenity }}</p>\n                              </div>\n                            </div>\n                          </div>\n                        </div>\n                    </div>\n\n                    <div class=\"col-md-4 mt-4\">\n                      <div class=\"card bg-light\">\n                        <div class=\"card-body\">\n                          <h4 class=\"text-dark\">Interested in this property?</h4>\n                          <small class=\"text-muted\">Get in touch today</small>\n\n                          <form action=\"#\" @submit.prevent=\"handleSubmit()\">\n\n                            <status v-model:status_data=status_data></status>\n\n                            <div class=\"form-row mt-4\">\n                              <div class=\"input-group\">\n                                  <span class=\"input-group-text border-end-0 bg-white\" style=\"border: 1px solid #eee\">\n                                      <font-awesome-icon class=\"ms-2\" id=\"eye\" :icon=\"['fa', 'user']\" />\n                                  </span>\n\n                                  <input v-model=\"form_data.names\" id=\"names\" type=\"text\" class=\"form-control border-start-0\" placeholder=\"Enter your Full Names\" required> \n                              </div>\n                            </div>\n\n                            <div class=\"form-row mt-4\">\n                              <div class=\"input-group\">\n                                  <span class=\"input-group-text border-end-0 bg-white\" style=\"border: 1px solid #eee\">\n                                      <font-awesome-icon class=\"ms-2\" id=\"eye\" :icon=\"['fa', 'envelope']\" />\n                                  </span>\n\n                                  <input v-model=\"form_data.email\" id=\"email\" type=\"email\" class=\"form-control border-start-0\" placeholder=\"Enter your Email.\" required> \n                              </div>\n                            </div> \n\n                            <div class=\"form-row mt-4\">\n                              <div class=\"input-group\">\n                                  <span class=\"input-group-text border-end-0 bg-white\" style=\"border: 1px solid #eee\">\n                                      <font-awesome-icon class=\"ms-2\" id=\"eye\" :icon=\"['fas', 'phone-alt']\" />\n                                  </span>\n\n                                  <input v-model=\"form_data.phone_number\" id=\"phone_number\" type=\"number\" class=\"form-control border-start-0\" placeholder=\"Enter your Phone Number\" required> \n                              </div>\n                            </div>\n\n                            <div class=\"form-row mt-4\">\n                              <div class=\"input-group\">\n                                  <span class=\"input-group-text border-end-0 bg-white\" style=\"border: 1px solid #eee\">\n                                      <font-awesome-icon class=\"ms-2\" id=\"eye\" :icon=\"['fas', 'phone-alt']\" />\n                                  </span>\n\n                                  <select v-model=\"form_data.purpose\" name=\"purpose\" id=\"purpose\" class=\"form-select border-start-0\">\n                                    <option>Book a tour or show interest in property</option>\n                                    <option value=\"tour\">Book House Tour</option>\n                                    <option value=\"purchase\">Interested in Purchase</option>\n                                  </select>\n                              </div>\n                            </div>                          \n\n                            <div class=\"form-row mt-4\">\n                              <div class=\"input-group\">\n                                  <span class=\"input-group-text border-end-0 bg-white\" style=\"border: 1px solid #eee\">\n                                      <font-awesome-icon class=\"ms-2\" id=\"eye\" :icon=\"['fas', 'comment']\" />\n                                  </span>\n\n                                  <textarea v-model=\"form_data.description\"  style=\"line-height: 1.5\" placeholder=\"You can write us a message here if you want to.\" class=\"form-control border-start-0\" name=\"description\" id=\"description\" rows=\"7\"></textarea>\n                              </div>\n                              <small class=\"text-muted\">Optional field</small>\n                            </div>   \n\n                            <button type=\"submit\" class=\"btn btn-success btn-lg w-100 mt-4\">\n                              <span class=\"text-uppercase\">Submit</span>\n                              <font-awesome-icon class=\"ms-3\" :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                            </button>   \n\n                          </form>                                                                    \n                        </div>\n                      </div>\n                    </div>\n                </div>\n            </section>\n\n            <main-footer v-if=\"page_props.footer\" :page=\"'Home'\" v-model:prop_data=\"page_props.footer\"></main-footer>\n        </div>\n    </div>\n</template>\n\n<style lang=\"scss\" scoped>\n  .body {\n    padding-left: 0!important;\n  }\n\n  .navbar-border-bottom {\n    border-bottom: 1px solid #fafafa !important;\n    box-shadow: 0 4px 8px rgb(0 0 0 / 3%);\n  }\n\n  .property {\n    padding-top: 1rem;\n    padding-bottom: 5rem;\n  }\n\n  .form-select, .form-control {\n    line-height: 3;\n    border: 1px solid #eee;\n  }\n\n  .card {\n    border: 1px solid #eee;\n  }\n\n  .image-backdrop {\n    position: absolute;\n    display: flex;\n    width: 100%;\n    height: 100%;\n    \n    &:hover {\n      z-index: 5;\n      background: #000;\n      opacity: 0.4;\n    }\n  }\n\n  .right-circle {\n    width: 45px;\n    height: 45px;\n    align-items: center;\n    justify-content: center;\n    border-radius: 50%;\n    font-size: 1rem;\n    z-index: 6;\n    position: absolute;\n    right: 2%;\n    top: 80%;\n    cursor: pointer;\n    padding: 1rem;\n  }\n\n  .left-circle {\n    width: 45px;\n    height: 45px;\n    align-items: center;\n    justify-content: center;\n    border-radius: 50%;\n    font-size: 1rem;\n    z-index: 6;\n    position: absolute;\n    left: 5%;\n    top: 80%;\n    cursor: pointer;\n    padding: 1rem;\n  }\n\n  .slide-fade-enter-active {\n    transition: all .3s ease-out;\n  }\n\n  .slide-fade-leave-active {\n    transition: all .1s cubic-bezier(1.0, 0.5, 0.8, 1.0);\n  }\n\n  .slide-fade-enter-from,\n  .slide-fade-leave-to {\n    transform: translateX(20px);\n    opacity: 0;\n  }\n\n</style>\n\n<script>\n  import Breadcrumbs from '../../components/AutoBreadcrumbs'\n  import Navbar from '../../components/Custom/PropertyNavbar'\n  import MainFooter from '../../components/Footer'\n  import Status from '../../components/Status'\n\n  export default {\n    name: 'Home',\n    props: ['property_id'],\n    components: {\n      Navbar,\n      Breadcrumbs,\n      MainFooter,\n      Status\n    }, \n    computed: {\n      page_props () {\n        return this.$store.state.page_props\n      }\n    },      \n    data() {\n      return {\n        title: {status: true, title: 'Your Data', actions: true},\n        breadcrumbs_data: [\n          {title: 'Home', link: '', active: false},\n          {title: 'Property', active: true},\n        ],\n        screen: 1,\n        no_images: null,\n        caption: null,\n        property: {},\n        amenity_icons: {\n          'Wi-Fi': 'wifi',\n          'CCTV': 'video',\n          'Gym': 'dumbbell',\n          'Swimming Pool': 'swimming-pool',\n          '24 Hour Security': 'lock',\n          'Parking Spots': 'parking',\n          'Outdoor Space': 'spa',\n          'Elevator': 'sort',\n          'Generator': 'bolt',\n          'Borehole': 'hand-holding-water',\n\n        },\n        form_data: {\n          names: null,\n          email: null,\n          purpose: null,\n          phone_number: null,\n          description: null,\n          property: null\n        },\n        status_data: {\n          \"success\": false,\n          \"success_message\": 'Form Submission was successful!',\n          \"error\": false,\n          \"error_message\": 'There are some errors.',\n          \"loading\": false\n        },        \n      } \n    },\n    mounted() {\n      if(this.page_props.length == 0) {\n        this.$store.dispatch(\"getProps\");\n      } \n\n      this.getProperty()\n    },\n    methods: {\n      handleSubmit() {\n        this.status_data.loading = true\n        this.form_data.property = {\n          name: this.property.name,\n          id: this.property.id\n        }\n\n        this.axios.post('api/message/', this.form_data).then(() => {\n          this.status_data.loading = false\n          this.status_data.success = true\n          this.status_data.error = false\n          this.status_data.success_message = 'Success!'\n        }).catch((error) => {\n          this.status_data.loading = false\n          this.status_data.success = false\n          this.status_data.error = true\n          this.status_data.error_message = 'Error! ' + error.response.data.message\n\n        })\n      },\n      nextScreen() {\n        this.screen++\n        this.caption = this.property.property_images[this.screen-1].caption\n      },\n      previousScreen() {\n        this.screen--\n        this.caption = this.property.property_images[this.screen-1].caption\n      },\n      formatNumber(num) {\n        return num.toString().replace(/(\\d)(?=(\\d{3})+(?!\\d))/g, '$1,')\n      },\n      getProperty() {\n        this.axios.get('api/property/' + this.property_id).then((response) => {\n          console.log(response);\n          this.property = response.data.data\n          this.no_images = this.property.property_images.length\n          \n          if(this.no_images > 0) {\n            this.caption = this.property.property_images[this.screen-1].caption\n          }\n        }).catch(() => {\n          this.$router.push({name: 'Properties'});\n        })\n      }\n    }\n  }\n</script>\n"],"sourceRoot":""}]}