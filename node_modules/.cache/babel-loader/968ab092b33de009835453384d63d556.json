C,KAAK,IAAV,EAAgB;AACd,YAAM,IAAG,GAAI,YAAY,CAAC,OAAb,CAAqB,MAArB,CAAb;;AACA,YAAG,IAAH,EAAS;AACP,UAAA,YAAY,CAAC,UAAb,CAAwB,MAAxB;AACF;;AAEA,aAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,UAAA,IAAI,EAAE;AAAP,SAAlB;AACF;;AAEA,WAAK,OAAL,GAAe,KAAf;AACF;AAZK,GAzCM;AAuDb,EAAA,QAAQ,EAAE;AACR,IAAA,IADQ,kBACA;AACN,aAAO,KAAK,MAAL,CAAY,KAAZ,CAAkB,IAAzB;AACF;AAHQ,GAvDG;AA4Db,EAAA,OA5Da,qBA4DH;AACR,SAAK,MAAL,CAAY,QAAZ,CAAqB,SAArB;AACD;AA9DY,CAAf","sourcesContent":["<template>\n  <transition v-if=\" !loading\" name=\"fade\">\n    <div id=\"wrapper\">\n        <sidebar :name=\"user.name\" :page=\"'Admin'\" :sidebar_data=sidebar_data></sidebar>\n        \n        <div class=\"body\">\n          <div class=\"backdrop\" v-on:click=\"close()\"></div>\n            \n          <admin-header :title=\"'Manage Site'\" :tagline=\"'All your controls, in one place.'\" v-model:all_screens=all_screens v-model:screen=screen v-model:url=url  style=\"margin-bottom: 3rem;\"></admin-header>\n\n          <div class=\"container mb-4\">\n            <div class=\"content-wrapper\">\n              <div class=\"content mx-auto\">\n\n                <!--  List 1 -->\n                <auto-list v-if=\"url != null\" v-model:all_screens=all_screens v-model:screen=\"screen\" :title=title :url=\"url\" v-model:list_data=\"list_data\"></auto-list>\n\n              </div>                                          \n            </div>\n          </div>\n          \n          <footer class=\"main-footer d-flex p-4 pb-2\">\n            <div class=\"float-end mx-auto\">\n              Copyright &copy;  \n\n              Built on <a href=\"www.inshaka.ga\">Inshaka</a>\n            </div>\n          </footer>\n        </div>\n\n        <property-modal></property-modal>\n\n        <amenity-modal></amenity-modal>\n\n        <images-modal></images-modal>\n\n        <user-modal></user-modal>\n    </div>\n  </transition>\n</template>\n\n<style lang=\"scss\" scoped>\n  @import '../assets/scss/layout/_spinner';\n</style>\n\n<script>\nimport Sidebar from '../components/AutoSidebar'\nimport AdminHeader from '../components/Header'\nimport AutoList from '../components/AutoList'\nimport PropertyModal from '../components/Custom/PropertyModal';\nimport AmenityModal from '../components/Custom/AmenityModal';\nimport ImagesModal from '../components/Custom/ImagesModal';\nimport UserModal from '../components/Custom/UserModal';\n\nexport default {\n  name: 'Admin',\n  components: {\n    AdminHeader,\n    Sidebar,\n    AutoList,\n    PropertyModal,\n    AmenityModal,\n    ImagesModal,\n    UserModal\n  },\n  data() {\n    return {\n      url: 'api/users',\n      title: {status: true, title: 'Your Data', actions: [\n        {name: 'Add User', type: 'modal', action_url: 'user'},\n        {name: 'Add Property', type: 'modal', action_url: 'property'}\n      ]},\n      sidebar_data: [\n        {title: 'Admin', link: '/admin', if_children: false, icon: 'cog', modal: false},\n        {title: 'Edit Site', link: '', if_children: false, icon: 'home', modal: false},\n        {title: 'Logout', link: 'logout', if_children: false, icon: 'sign-out-alt', modal: false},\n      ],\n      all_screens: [\n        {title: 'Users', screen: 1, url: 'api/users', actions: [\n          {name: 'Edit', type: 'modal', action_url: 'user'},\n          {name: 'Delete', type: 'api', action_url: {type: 'delete', url:'api/users/', message: {success: 'Success!', error: 'Error!'}}},\n        ]},\n        {title: 'Property', screen: 2, url: 'api/property', actions: [\n          {name: 'Edit', type: 'modal', action_url: 'property'},\n          {name: 'Delete', type: 'api', action_url: {type: 'delete', url:'api/property/', message: {success: 'Success!', error: 'Error!'}}},\n          {name: 'Featured', type: 'api', action_url: {type: 'post', url:'api/featured/', message: {success: 'Featured update!', error: 'Error!'}}},\n          {name: 'Manage Images', type: 'modal', action_url: 'images'},\n          {name: 'Manage Amenities', type: 'modal', action_url: 'amenity'},\n        ]},\n      ],\n      screen: 1,\n      list_data: [],\n      loading: true,\n    } \n  },\n  watch: {\n    user: function() {\n      if( !this.user) {\n        const user = localStorage.getItem('user')\n        if(user) {\n          localStorage.removeItem('user')\n        }\n\n        this.$router.push({name: 'Home'});\n      }\n\n      this.loading = false\n    }\n  },\n  computed: {\n    user () {\n      return this.$store.state.user\n    }\n  },      \n  mounted() {\n    this.$store.dispatch(\"getUser\")\n  },\n}\n</script>\n"],"sourceRoot":""}]}