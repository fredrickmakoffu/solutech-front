,EAAmC;AAC/B,cAAG,MAAI,CAAC,SAAL,CAAe,cAAf,CAA8B,IAA9B,IAAsC,OAAO,CAAC,CAAD,CAAP,CAAW,IAApD,EAA0D;AACtD,YAAA,cAAc,CAAC,IAAf,CAAoB;AAChB,cAAA,KAAK,EAAE,OAAO,CAAC,CAAD,CAAP,CAAW,IADF;AAEhB,cAAA,IAAI,EAAE,MAFU;AAGhB,cAAA,SAAS,EAAE,WAHK;AAIhB,cAAA,SAAS,EAAE,KAJK;AAKhB,cAAA,IAAI,EAAE,MALU;AAMhB,cAAA,KAAK,EAAE,IANS;AAOhB,cAAA,MAAM,EAAE;AAPQ,aAApB;AASJ;;AAEA,cAAG,MAAI,CAAC,SAAL,CAAe,cAAf,CAA8B,IAA9B,IAAsC,OAAO,CAAC,CAAD,CAAP,CAAW,IAApD,EAA0D;AACtD,YAAA,cAAc,CAAC,IAAf,CAAoB;AAChB,cAAA,KAAK,EAAE,OAAO,CAAC,CAAD,CAAP,CAAW,IADF;AAEhB,cAAA,IAAI,EAAE,MAFU;AAGhB,cAAA,SAAS,EAAE,WAHK;AAIhB,cAAA,SAAS,EAAE,KAJK;AAKhB,cAAA,IAAI,EAAE,MALU;AAMhB,cAAA,KAAK,EAAE,IANS;AAOhB,cAAA,MAAM,EAAE;AAPQ,aAApB;AASJ;;AAEA,cAAG,MAAI,CAAC,SAAL,CAAe,cAAf,CAA8B,gBAA9B,IAAkD,OAAO,CAAC,CAAD,CAAP,CAAW,gBAAhE,EAAkF;AAC9E,YAAA,cAAc,CAAC,IAAf,CAAoB;AAChB,cAAA,KAAK,EAAE,OAAO,CAAC,CAAD,CAAP,CAAW,gBADF;AAEhB,cAAA,IAAI,EAAE,kBAFU;AAGhB,cAAA,SAAS,EAAE,WAHK;AAIhB,cAAA,SAAS,EAAE,KAJK;AAKhB,cAAA,IAAI,EAAE,MALU;AAMhB,cAAA,KAAK,EAAE,IANS;AAOhB,cAAA,MAAM,EAAE;AAPQ,aAApB;AASJ;;AAEA,cAAG,MAAI,CAAC,SAAL,CAAe,cAAf,CAA8B,IAA9B,IAAsC,OAAO,CAAC,CAAD,CAAP,CAAW,IAApD,EAA0D;AACtD,YAAA,cAAc,CAAC,IAAf,CAAoB;AAChB,cAAA,KAAK,EAAE,OAAO,CAAC,CAAD,CAAP,CAAW,IADF;AAEhB,cAAA,IAAI,EAAE,MAFU;AAGhB,cAAA,SAAS,EAAE,WAHK;AAIhB,cAAA,SAAS,EAAE,KAJK;AAKhB,cAAA,IAAI,EAAE,MALU;AAMhB,cAAA,KAAK,EAAE,IANS;AAOhB,cAAA,MAAM,EAAE;AAPQ,aAApB;AASJ;AAEJ,SAjDA,MAiDO;AACH,cAAG,MAAI,CAAC,SAAL,CAAe,OAAO,CAAC,CAAD,CAAtB,KAA8B,OAAO,CAAC,CAAD,CAAxC,EAA6C;AACzC,YAAA,cAAc,CAAC,IAAf,CAAoB;AAChB,cAAA,KAAK,EAAE,OAAO,CAAC,CAAD,CADE;AAEhB,cAAA,IAAI,EAAE,OAAO,CAAC,CAAD,CAFG;AAGhB,cAAA,SAAS,EAAE,WAHK;AAIhB,cAAA,SAAS,EAAE,KAJK;AAKhB,cAAA,IAAI,EAAE,MALU;AAMhB,cAAA,KAAK,EAAE,KANS;AAOhB,cAAA,MAAM,EAAE;AAPQ,aAApB;AASJ;AACJ;AACH,OA/DD;AAiEA,aAAO,cAAP;AACH,KA7FI;AA8FL,IAAA,WA9FK,yBA8FS;AAAA;;AACV,WAAK,OAAL,GAAe,IAAf;AACA,WAAK,KAAL,CAAW,GAAX,CAAe,cAAf,EAA+B,IAA/B,CAAoC,UAAC,QAAD,EAAc;AAC9C,QAAA,MAAI,CAAC,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,IAA9B;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACH,OAHD,EAGG,KAHH,CAGS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,OALD;AAMH,KAtGI;AAuGL,IAAA,UAvGK,wBAuGQ;AACT,UAAG,YAAY,CAAC,OAAb,CAAqB,MAArB,KAAgC,IAAnC,EAAyC;AACrC,aAAK,YAAL,GAAoB,IAApB;AACJ,OAFA,MAEO;AACH,aAAK,YAAL,GAAoB,KAApB;AACJ;AACJ;AA7GK;AAzEE,CAAf","sourcesContent":["<template>\n    <div class=\"container\">\n        <div class=\"position-absolute cog p-4\" v-if=\"if_logged_in\">\n            <button class=\"btn btn-dark bg-gradient btn-sm\" data-bs-toggle=\"modal\" data-bs-target=\"#catalogueProps\">\n                <font-awesome-icon :icon=\"['fas', 'cogs']\"></font-awesome-icon>\n            </button>\n        </div>\n\n        <div class=\"row ps-4 pe-4\">\n            <div class=\"col-12 text-center mb-4\">\n                <h2 class=\"text-burgundy fw-bold burgundy-gradient-text\">\n                    {{ prop_data.title }}\n                </h2>\n                <p style=\"font-size: 15px; letter-spacing: 0.4px;\">{{ prop_data.tagline }}</p>\n            </div>           \n        </div>\n\n        <div class=\"row ps-4 pe-4\" v-if=\"featured\">\n            <div class=\"col-md-4 col-sm-12 mt-4\" v-for=\"({name, id, address, image_url, bedrooms, bathrooms, sale_type, index}) in featured\" :key=\"index\">\n\n                <div class=\"card rounded\" style=\"box-shadow: 0 4px 8px rgb(0 0 0 / 3%); border-color: #eee;\">\n                    <img v-if=\"image_url != null\" :src=\"axios.defaults.baseURL + '/' + image_url\" alt=\"\">\n                    \n                    <div class=\"card-body\">\n                        <router-link class=\"text-decoration-none\" :to=\"{name: 'Property', params: {property_id: id}}\">\n                            <h6 class=\"burgundy-gradient-text fw-bold\">{{ name }}</h6>\n                        </router-link>\n                        \n                        <p class=\"text-muted\">{{ bedrooms }} bedrooms, {{ bathrooms }} bathrooms</p>\n\n                        <div class=\"d-flex mt-4\">\n                            <p>&bull; {{ address }}</p>\n                            <p :class=\"['ms-auto fw-bold text-capitalize ', \n                            {'text-burgundy': sale_type == 'build'}, {'text-success': sale_type == 'buy'}\n                            ]\">&bull; {{ sale_type }}</p>\n                        </div>\n                    </div>\n                </div>\n            </div>                                                                                                   \n        </div>\n\n        <div class=\"row ps-4 pe-4 d-flex\" v-if=\"featured.length == 0\">\n            <no-data :message=\"'No property listed yet.'\"></no-data>\n        </div>\n\n        <div class=\"row mt-4 ps-4 pe-4\">\n            <div class=\"col-md-12 d-flex\">\n                <router-link :to=\"'/properties/build'\" :class=\"['btn', 'bg-' + prop_data.call_to_action.background_color, 'w-15', 'ms-auto', 'text-white', 'mt-4']\">\n\n                    <span> {{ prop_data.call_to_action.text }} </span>\n                    <font-awesome-icon class=\"ms-2\" :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                    \n                </router-link>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"modal fade\" id=\"catalogueProps\" tabindex=\"-1\" aria-labelledby=\"cataloguePropsLabel\" aria-hidden=\"true\">\n        <div class=\"modal-dialog\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header pb-0 border-bottom-0\">\n                    <span class=\"d-block container\">\n                        <h5 class=\"text-dark text-center\"><span class=\"text-burgundy fw-bold\">Catalogue</span> Section</h5>\n                    </span>\n                </div>\n                \n                <div class=\"modal-body container\">\n                    <status v-model:status_data=status_data></status>\n\n                    <form class=\"row\" action=\"#\" @submit.prevent=\"submit(form_data, [false, ''], 'catalogue')\" v-show=\"form_screen == 1\">\n                        <div class=\"row mb-4\">\n                            <label for=\"title\" class=\"fw-bold mb-2\">Title</label>\n                            <div class=\"input-group\">\n                                <input id=\"title\" type=\"text\" class=\"form-control\" v-model=\"form_data.title\" required>\n                            </div>\n                            <small class=\"text-muted\">Enter your title.</small>\n                        </div>\n\n                        <div class=\"row mb-4\">\n                            <label for=\"tagline\" class=\"fw-bold mb-2\">Tagline</label>\n                            <div class=\"input-group\">\n                                <input id=\"tagline\" type=\"text\" class=\"form-control\" v-model=\"form_data.tagline\" required> \n                            </div>\n                            <small class=\"text-muted\">Enter your tagline.</small>\n                        </div>\n\n                        <div class=\"row mb-4\">\n                            <label for=\"image\" class=\"fw-bold mb-2\">Background color</label>\n\n                            <div class=\"input-group\">\n                                <select id=\"background_color\" class=\"form-select\" v-model=\"form_data.background_color\" required>\n                                    <option></option>\n                                    <option value=\"white\">White</option>\n                                    <option value=\"light\">Light</option>\n                                    <option value=\"dark\">Dark</option>\n                                    <option value=\"burgundy\">Burgundy</option>\n                                </select>\n                            </div>\n                            <small class=\"text-muted\">Enter the background color.</small>\n                        </div>\n\n                        <div class=\"row mt-2\">\n                            <div class=\"col-md-12 mt-2 d-flex\">\n                                <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                    Submit\n                                </button>\n\n                                <button type=\"button\" class=\"btn bg-secondary text-white w-25\" @click=\"changeScreen(2)\">\n                                    <font-awesome-icon :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                                </button>\n                            </div>\n                        </div> \n                    </form>\n\n                    <form class=\"row\" action=\"#\" @submit.prevent=\"submit(call_to_action, [true, 'call_to_action'], 'catalogue')\" v-show=\"form_screen == 2\">\n                        <div class=\"row\"> \n                            <div class=\"col-md-12 col-sm-12 mb-4\">\n                                <label for=\"call_to_action_background\" class=\"fw-bold mb-2\">Button Color</label>\n\n                                <div class=\"input-group\">\n                                    <select id=\"call_to_action_background\" class=\"form-select\" v-model=\"call_to_action.background_color\" required>\n                                        <option value=\"light\">Light</option>\n                                        <option value=\"dark\">Dark</option>\n                                        <option value=\"burgundy\">Burgundy</option>\n                                    </select> \n                                </div>\n                                <small class=\"text-muted\">Enter the button background color.</small>\n                            </div>\n\n                            <div class=\"col-md-12 col-sm-12 mb-4\">\n                                <label for=\"call_to_action_text\" class=\"fw-bold mb-2\">Button Text</label>\n\n                                <div class=\"input-group\">\n                                    <input id=\"call_to_action_text\" type=\"text\" class=\"form-control\" v-model=\"call_to_action.text\" required> \n                                </div>\n                                <small class=\"text-muted\">Enter the text on button.</small>\n                            </div>\n\n                            <div class=\"col-md-12 col-sm-12 mb-4\">\n                                <label for=\"call_to_action_link\" class=\"fw-bold mb-2\">Button Link</label>\n\n                                <div class=\"input-group\">\n                                    <input id=\"call_to_action_link\" type=\"text\" class=\"form-control\" v-model=\"call_to_action.link\" required>\n                                </div>\n                                <small class=\"text-muted\">Enter the link the button leads to.</small>\n                            </div>\n\n\n                            <div class=\"col-md-12 mt-2 d-flex\">\n                                <button type=\"button\" class=\"btn bg-secondary text-white w-25 me-2\" @click=\"changeScreen(1)\">\n                                    <font-awesome-icon :icon=\"['fas', 'angle-left']\"></font-awesome-icon>\n                                </button>\n\n                                <button type=\"submit\" class=\"btn btn-dark w-75\">\n                                    Submit\n                                </button>\n                            </div>  \n                        </div>\n                    </form>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import Status from '../../Status';\n    import handleProps from '../../../mixins/handleProps';\n    import NoData from '../../NoData'    \n\n    export default {\n        name: \"CataloguePreview\",\n        mixins: [handleProps],\n        components: {\n            Status,\n            NoData\n        },\n        props: { \n            prop_data: {\n                required: true,\n                type: Object\n            },\n            page: {\n                required: true, \n                type: String\n            }        \n        },\n        data() {\n            return {\n                filter: false,\n                form_data: {\n                    title: this.prop_data.title,\n                    tagline: this.prop_data.tagline,\n                    background_color: this.prop_data.background_color,\n                },\n                call_to_action: {\n                    text: this.prop_data.call_to_action.text,\n                    link: this.prop_data.call_to_action.link,\n                    background_color: this.prop_data.call_to_action.background_color\n                },                \n                catalogue_item: [\n                    {\n                        image: \"https://images.unsplash.com/photo-1618221195710-dd6b41faaea6?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1549&q=80\",\n                        title: \"Furnished 1 Bedroom Apartment For Sale In Kileleshwa\",\n                        tagline: \"Kileleshwa, Nairobi, 97104, Kenya\",\n                        left_small: \"Villa\",\n                        right_small: \"07-07-2021\",\n                        link: \"\"\n                    },\n                    {\n                        image: \"https://images.unsplash.com/photo-1618221195710-dd6b41faaea6?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1549&q=80\",\n                        title: \"Furnished 1 Bedroom Apartment For Sale In Kileleshwa\",\n                        tagline: \"Kileleshwa, Nairobi, 97104, Kenya\",\n                        left_small: \"Villa\",\n                        right_small: \"07-07-2021\",\n                        link: \"\"\n                    },\n                    {\n                        image: \"https://images.unsplash.com/photo-1618221195710-dd6b41faaea6?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1549&q=80\",\n                        title: \"Furnished 1 Bedroom Apartment For Sale In Kileleshwa\",\n                        tagline: \"Kileleshwa, Nairobi, 97104, Kenya\",\n                        left_small: \"Villa\",\n                        right_small: \"07-07-2021\",\n                        link: \"\"\n                    }                        \n                ],\n                status_data: {\n                    success: false,\n                    success_message: 'Form Submission was successful!',\n                    error: false,\n                    error_message: 'There are some errors.',\n                    loading: false\n                },   \n                form_screen: 1,\n                featured: {},\n                loading: false,\n                if_logged_in: false     \n            }\n        },\n        mounted() {\n            this.getFeatured()\n            this.ifLoggedIn()\n        },\n        methods: {\n            showFilter() {\n                if(this.filter == true) {\n                    this.filter = false\n                } else {\n                    this.filter = true\n                }\n            },\n            handleSubmit() {\n                const post_form_data = this.prepFormData()\n                \n                this.axios.post('api/prop', post_form_data).then(() => {\n                    this.status_data.loading = false\n                    this.status_data.error = false\n                    this.status_data.success = true\n\n                    this.$emit('update:prop_data', this.form_data)\n                }).catch(() => {\n                    this.status_data.loading = false\n                    this.status_data.error = true\n                    this.status_data.error_message = 'Section data was not successfully updated!'\n                    this.status_data.success = false\n                })\n            },\n            prepFormData() {\n                var post_form_data = []\n                \n                Object.entries(this.form_data).forEach(element => {\n                    if(element[0] == 'call_to_action') {\n                        if(this.prop_data.call_to_action.text != element[1].text) {\n                            post_form_data.push({\n                                value: element[1].text,\n                                prop: 'text',\n                                component: 'catalogue',\n                                container: false,\n                                page: 'Home',\n                                child: true,\n                                parent: \"call_to_action\"\n                            })\n                        }\n\n                        if(this.prop_data.call_to_action.link != element[1].link) {\n                            post_form_data.push({\n                                value: element[1].link,\n                                prop: 'link',\n                                component: 'catalogue',\n                                container: false,\n                                page: 'Home',\n                                child: true,\n                                parent: \"call_to_action\"\n                            })\n                        }\n\n                        if(this.prop_data.call_to_action.background_color != element[1].background_color) {\n                            post_form_data.push({\n                                value: element[1].background_color,\n                                prop: 'background_color',\n                                component: 'catalogue',\n                                container: false,\n                                page: 'Home',\n                                child: true,\n                                parent: \"call_to_action\"\n                            })\n                        }\n\n                        if(this.prop_data.call_to_action.icon != element[1].icon) {\n                            post_form_data.push({\n                                value: element[1].icon,\n                                prop: 'icon',\n                                component: 'catalogue',\n                                container: false,\n                                page: 'Home',\n                                child: true,\n                                parent: \"call_to_action\"\n                            })\n                        }\n\n                    } else {\n                        if(this.prop_data[element[0]] != element[1]) {\n                            post_form_data.push({\n                                value: element[1],\n                                prop: element[0],\n                                component: 'catalogue',\n                                container: false,\n                                page: 'Home',\n                                child: false,\n                                parent: \"\"\n                            })\n                        }\n                    }\n                });\n\n                return post_form_data\n            },\n            getFeatured() {\n                this.loading = true\n                this.axios.get('api/featured').then((response) => {\n                    this.featured = response.data.data\n                    this.loading = false\n                }).catch((error) => {\n                    console.log(error)\n                })\n            },\n            ifLoggedIn() {\n                if(localStorage.getItem('user') != null) {\n                    this.if_logged_in = true\n                } else {\n                    this.if_logged_in = false\n                }\n            }             \n        }\n    }\n</script>"],"sourceRoot":""}]}