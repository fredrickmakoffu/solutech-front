2B,IAA3B;AACA,QAAA,KAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,KAAzB;AAEA,QAAA,YAAY,CAAC,OAAb,CAAqB,MAArB,EAA6B,IAAI,CAAC,SAAL,CAAe;AACxC,mBAAS,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,KADY;AAExC,oBAAU,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB;AAFW,SAAf,CAA7B;AAKA,QAAA,UAAU,CAAC,YAAM;AACb,UAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAC,YAAA,IAAI,EAAE;AAAP,WAAlB;AACH,SAFS,EAEP,IAFO,CAAV;AAGH,OAbD,EAaG,KAbH,CAaS,UAAC,KAAD,EAAW;AAChB,QAAA,KAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACA,QAAA,KAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACA,QAAA,KAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,IAAzB;AAEA,QAAA,KAAI,CAAC,WAAL,CAAiB,aAAjB,GAAiC,KAAjC;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,OApBD;AAqBH,KAzBI;AA0BL,IAAA,WA1BK,yBA0BS;AACV,UAAG,QAAQ,CAAC,aAAT,CAAuB,WAAvB,EAAoC,IAApC,IAA4C,UAA/C,EAA2D;AACvD,QAAA,QAAQ,CAAC,aAAT,CAAuB,WAAvB,EAAoC,IAApC,GAA2C,MAA3C;AACJ,OAFA,MAEO;AACH,QAAA,QAAQ,CAAC,aAAT,CAAuB,WAAvB,EAAoC,IAApC,GAA2C,UAA3C;AACJ;AACH,KAhCI;AAiCL,IAAA,SAjCK,uBAiCO;AACR,UAAG,KAAK,WAAL,CAAiB,OAAjB,IAA4B,IAA/B,EAAqC;AACjC,aAAK,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACJ,OAFA,MAEO,IAAG,KAAK,WAAL,CAAiB,KAAjB,IAA0B,IAA7B,EAAmC;AACtC,aAAK,WAAL,CAAiB,KAAjB,GAAyB,KAAzB;AACJ;AACJ;AAvCK;AArBE,CAAf","sourcesContent":["<template>\n    <div id=\"wrapper\" class=\"bg-light pt-4\">\n        <div class=\"container-fluid\">\n            <div class=\"row\">\n                <div class=\"col-xl-4 offset-xl-4 col-lg-4 col-md-5 offset-lg-4 offset-md-2\">\n                    <div class=\"card bg-gradient border-0\">\n                        <!-- strip of black at the top of the login box -->\n                        <div class=\"bg-dark pt-1\"></div>\n\n                        <logo class=\"pt-4\"></logo>\n\n                        <status v-model:status_data=status_data></status>\n\n                        <form id=\"form\" action=\"#\" @submit.prevent=\"handleSubmit()\">\n                            <div class=\"card-body border-bottom-0 p-4\">\n                                <div class=\"form-row\">\n                                    <div class=\"input-group\">\n                                        <input id=\"email\" type=\"email\" class=\"form-control no-border\" v-model=\"form_data.email\" placeholder=\"Enter your Email\" required>\n\n                                        <span class=\"input-group-text pe-4 border-right-0\" style=\"border-bottom: 1px #eee solid;\" @click=\"seePassword()\">\n                                            <font-awesome-icon id=\"eye\" :icon=\"['fa', 'envelope']\" />\n                                        </span>\n                                    </div>\n                                </div>\n\n                                <div class=\"row mt-4\" id=\"password-row\">\n                                    <div class=\"input-group\">\n                                        <input id=\"password\" type=\"password\" class=\"form-control\" v-model=\"form_data.password\" placeholder=\"Password\" required>\n                                        <span class=\"input-group-text pe-4\" style=\"border-right: 1px #eee solid; border-bottom: 1px #eee solid;\" @click=\"seePassword()\">\n                                            <font-awesome-icon id=\"eye\" :icon=\"['fa', 'eye']\" />\n                                        </span>\n\n                                        <span class=\"input-group-text ps-4\" style=\"border-bottom: 1px #eee solid;\">\n                                            <font-awesome-icon id=\"lock\" :icon=\"['fa', 'lock']\" />\n                                        </span>\n                                    </div>\n                                    <router-link :to=\"'/forgot-password'\">\n                                        <small class=\"text-muted\">\n                                            <a href=\"#\" class=\"text-dark fw-bold text-decoration-none float-end mt-2\">Forgot Password</a>\n                                        </small>\n                                    </router-link>\n                                </div>\n\n                                <div class=\"form-row mt-3 mb-3\">\n                                    <input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"flexCheckDefault\">\n                                    <label class=\"form-check-label ms-2\" for=\"flexCheckDefault\">\n                                        Remember me\n                                    </label>\n                                </div>\n\n                                <div class=\"row\" id=\"login\">\n                                    <div class=\"col\">\n                                        <button class=\"btn btn-dark btn-sm mt-4 w-100 text-uppercase fw-bold pt-2 pb-2\" type=\"submit\">\n                                            <span style=\"letter-spacing: 2px\">Login</span> <font-awesome-icon class=\"icon ms-1 me-1\" :icon=\"['fa', 'unlock']\" />\n                                        </button>\n                                    </div>\n                                </div>\n                            </div>\n                        </form>\n                    </div>\n                </div>  \n            </div>\n        </div>\n    </div>\n</template>\n\n<style scoped lang=\"scss\">\n    @import '../assets/scss/base/_base.scss';\n    @import '../assets/scss/base/_fonts.scss';\n    @import '../assets/scss/helpers/breakpoints.scss';\n\n    .card {\n        box-shadow: 0 4px 8px rgb(0 0 0 / 3%)\n    }\n    \n</style>\n\n<script>\n    import Status from '../components/Status';\n    import Logo from '../components/Logo';\n\n    export default {\n        name: 'Login',\n        components: {\n            Status,\n            Logo\n        },\n        data() {\n            return {\n                form_data: {\n                    email: '',\n                    password: '',\n                },\n                status_data: {\n                    \"success\": false,\n                    \"success_message\": 'You\\'re in! Login is Successful!',\n                    \"error\": false,\n                    \"error_message\": 'There are some input errors.',\n                },\n                loading: false\n            }\n        },\n        methods: {\n            handleSubmit() {\n                this.status_data.loading = true\n\n                this.axios.post('api/sanctum/token', this.form_data).then((response) => {\n                    this.status_data.loading = false\n                    this.status_data.success = true\n                    this.status_data.error = false\n\n                    localStorage.setItem('user', JSON.stringify({\n                        'token': response.data.data.token, \n                        'expiry': response.data.data.expiry\n                    }))\n\n                    setTimeout(() => {\n                        this.$router.push({name: 'Admin'});\n                    }, 2000);\n                }).catch((error) => {\n                    this.status_data.loading = false\n                    this.status_data.success = false\n                    this.status_data.error = true\n\n                    this.status_data.error_message = error\n                    console.log(error);\n                })      \n            },\n            seePassword() {\n                if(document.querySelector('#password').type == 'password') {\n                    document.querySelector('#password').type = 'text'\n                } else {\n                    document.querySelector('#password').type = 'password'\n                }\n            },\n            hideAlert() {\n                if(this.status_data.success == true) {\n                    this.status_data.success = false\n                } else if(this.status_data.error == true) {\n                    this.status_data.error = false\n                }\n            }\n        }\n    }\n</script>"],"sourceRoot":""}]}