AAE,KAAvB;AAA8B,UAAA,UAAU,EAAE;AAAC,YAAA,IAAI,EAAE,QAAP;AAAiB,YAAA,GAAG,EAAC,eAArB;AAAsC,YAAA,OAAO,EAAE;AAAC,cAAA,OAAO,EAAE,UAAV;AAAsB,cAAA,KAAK,EAAE;AAA7B;AAA/C;AAA1C,SAF+D;AAAjE,OADW,CALR;AAWL,MAAA,MAAM,EAAE,CAXH;AAYL,MAAA,SAAS,EAAE,EAZN;AAaL,MAAA,OAAO,EAAE,IAbJ;AAcL,MAAA,OAAO,EAAE;AAdJ,KAAP;AAgBD,GA1BY;AA2Bb,EAAA,OA3Ba,qBA2BH;AACR,SAAK,UAAL;AACA,SAAK,UAAL;AACD,GA9BY;AA+Bb,EAAA,OAAO,EAAE;AACP,IAAA,UADO,wBACM;AACX,WAAK,OAAL,GAAe,IAAf;;AAEA,UAAG,YAAY,CAAC,OAAb,CAAqB,MAArB,CAAH,EAAiC;AAC/B,aAAK,OAAL,GAAe,KAAf;AACF,OAFA,MAEO;AACL,aAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,UAAA,IAAI,EAAE;AAAP,SAAlB;AACA,aAAK,OAAL,GAAe,KAAf;AACF;AACD,KAVM;AAWP,IAAA,UAXO,wBAWM;AAAA;;AACX,WAAK,KAAL,CAAW,GAAX,CAAe,sBAAf,EAAuC,IAAvC,CAA4C,UAAC,QAAD,EAAc;AACxD,QAAA,KAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,IAA7B;AACD,OAFD,EAEG,KAFH,CAES,UAAC,MAAD,EAAY;AACnB,QAAA,OAAO,CAAC,GAAR,CAAY,MAAZ;AACD,OAJD;AAKD;AAjBM;AA/BI,CAAf","sourcesContent":["<template>\n  <div id=\"wrapper\" v-if=\"!loading\">\n      <sidebar :page=\"'Vehicles'\"></sidebar>\n      \n      <div class=\"body\">\n        <div class=\"backdrop\" v-on:click=\"close()\"></div>\n          \n        <admin-header :title=\"'Manage Vehicles'\" :tagline=\"'Manage your vehicles from here.'\" v-model:all_screens=all_screens v-model:screen=screen v-model:url=url  style=\"margin-bottom: 3rem;\"></admin-header>\n\n        <div class=\"container mb-4\">\n          <div class=\"content-wrapper\">\n            <div class=\"content mx-auto\">\n              <div class=\"row\">\n                <div class=\"col-md-3 col-sm-6 mt-4\">\n                  <div class=\"card bg-primary border-0 p-4\">\n                    <h2 class=\"text-white fw-bold\">{{ reports.total }}</h2>\n                    <p class=\"mb-0 text-light\">Total Vehicles</p>\n                  </div>\n                </div>\n\n                <div class=\"col-md-3 col-sm-6 mt-4\">\n                  <div class=\"card bg-info border-0 p-4\">\n                    <h2 class=\"text-dark fw-bold\">{{ reports.loading }}</h2>\n                    <p class=\"mb-0 text-dark\">Vehicles Loading</p>\n                  </div>\n                </div>\n\n                <div class=\"col-md-3 col-sm-6 mt-4\">\n                  <div class=\"card bg-primary border-0 p-4\">\n                    <h2 class=\"text-white fw-bold\">{{ reports.on_transit }}</h2>\n                    <p class=\"mb-0 text-light\">Vehicles on Transit</p>\n                  </div>\n                </div>\n              </div>\n\n              <!--  List 1 -->\n              <auto-list v-if=\"url != null\" v-model:all_screens=all_screens v-model:screen=\"screen\" :title=title :url=\"url\" v-model:list_data=\"list_data\"></auto-list>\n\n            </div>                                          \n          </div>\n        </div>\n        \n        <footer class=\"main-footer d-flex p-4 pb-2\">\n          <div class=\"float-end mx-auto\">\n            Copyright &copy;  \n\n            Built for <a href=\"#\">Solutech</a>\n          </div>\n        </footer>\n      </div>\n\n      <vehicle-modal v-model:list_data=list_data></vehicle-modal>\n  </div>\n</template>\n\n<style lang=\"scss\" scoped>\n  @import '../assets/scss/layout/_spinner';\n</style>\n\n<script>\nimport Sidebar from '../components/Sidebar'\nimport AdminHeader from '../components/Header'\nimport AutoList from '../components/AutoList'\nimport ifLoggedIn from '../mixins/ifLoggedIn'\nimport VehicleModal from '../components/Modal/VehicleModal'\n\nexport default {\n  name: 'Orders',\n  mixins: [ifLoggedIn],\n  components: {\n    AdminHeader,\n    Sidebar,\n    AutoList,\n    VehicleModal\n  },\n  data() {\n    return {\n      url: 'api/vehicles',\n      title: {status: true, title: 'Your Data', actions: [\n        {name: 'Add Vehicle', type: 'modal', action_url: 'vehicle'}\n      ]},\n      all_screens: [\n        {title: 'All Vehicles', screen: 1, url: 'api/vehicles', actions: [\n          {name: 'Edit', type: 'modal', action_url: 'vehicle'},\n          {name: 'Delete', type: 'api', action_url: {type: 'delete', url:'api/vehicles/', message: {success: 'Success!', error: 'Error!'}}},\n        ]},\n      ],\n      screen: 1,\n      list_data: [],\n      loading: true,\n      reports: []\n    } \n  },  \n  mounted() {\n    this.ifLoggedIn()\n    this.getReports()\n  },\n  methods: {\n    ifLoggedIn() {\n      this.loading = true\n\n      if(localStorage.getItem('user')) {\n        this.loading = false\n      } else {\n        this.$router.push({name: 'Login'})\n        this.loading = false\n      }\n    },\n    getReports() {\n      this.axios.get('/api/vehicle-reports').then((response) => {\n        this.reports = response.data.data\n      }).catch((errors) => {\n        console.log(errors);\n      })  \n    },     \n  }\n}\n</script>\n"],"sourceRoot":""}]}