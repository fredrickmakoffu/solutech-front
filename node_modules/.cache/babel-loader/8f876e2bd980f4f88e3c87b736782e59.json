D,EAAW;AAClBC,kBAAAA,OAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,iBAJD;;AAD2B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAM5B,KAhCM;AAiCPK,IAAAA,YAjCO,wBAiCMjB,OAjCN,EAiCeI,IAjCf,EAiCqB;AAC1BJ,MAAAA,OAAO,CAACS,MAAR,CAAe,cAAf,EAA+BL,IAA/B;AACD;AAnCM,GA3Ba;AAgEtBc,EAAAA,OAAO,EAAE;AACP/B,IAAAA,UAAU,EAAE,oBAAAD,KAAK;AAAA,aAAIA,KAAK,CAACC,UAAV;AAAA,KADV;AAEPE,IAAAA,QAAQ,EAAE,kBAAAH,KAAK;AAAA,aAAIA,KAAK,CAACG,QAAV;AAAA,KAFR;AAGPC,IAAAA,UAAU,EAAE,oBAAAJ,KAAK;AAAA,aAAIA,KAAK,CAACI,UAAV;AAAA,KAHV;AAIPC,IAAAA,UAAU,EAAE,oBAAAL,KAAK;AAAA,aAAIA,KAAK,CAACK,UAAV;AAAA;AAJV;AAhEa,CAAD,CAAzB;AAwEAjB,SAAS,CAACC,GAAD,CAAT,CACK4C,GADL,CACS3C,MADT,EAEK2C,GAFL,CAESzC,QAFT,EAEmBD,KAFnB,EAGK0C,GAHL,CAGSlC,KAHT,EAIKmC,SAJL,CAIe,mBAJf,EAIoCxC,eAJpC,EAKKyC,KALL,CAKW,MALX","sourcesContent":["import { createApp } from 'vue'\nimport App from './App.vue'\nimport router from './router'\nimport axios from 'axios'\nimport VueAxios from 'vue-axios'\nimport { createStore } from 'vuex'\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome'\nimport 'bootstrap'\nimport 'bootstrap/dist/css/bootstrap.min.css'\n\n\n// Axios defaults\naxios.defaults.headers.common['Accept'] = 'application/json';\naxios.defaults.baseURL = 'https://api.kipekee.co.ke';\n// axios.defaults.baseURL = 'http://localhost:8000';\n\n// store instance setup\nconst store = createStore({\n    state () {\n      return {\n        page_props: [],\n        loading: true,\n        featured: {},\n        properties: {},\n        modal_list: {}\n      }\n    },\n    mutations: {\n      setProps(state, page_props) {\n        state.page_props = page_props;\n      },\n      setLoading(state, loading) {\n        state.loading = loading\n      },\n      setFeatured(state, featured) {\n        state.featured = featured\n      },\n      setProperty(state, properties) {\n        state.properties = properties\n      },\n      setModalList(state, modal_list) {\n        state.modal_list = modal_list\n      }      \n    },\n    actions: {\n      async getProps(context) {\n        axios.get('api/page-prop?page=Home').then((response) => {\n          const page_props = response.data.data\n          page_props.services['service-items'] = page_props['service-items']\n          page_props.partners['partner-items'] = page_props['partner-items']\n          page_props.testimonials['testimonial-items'] = page_props['testimonial-items']\n\n          // Footer\n          page_props.footer['socials'] = page_props['socials']\n          page_props.footer['navigation'] = page_props['navigation']\n          context.commit(\"setProps\", page_props);\n\n          setTimeout(() => (context.commit(\"setLoading\", false)), 2000)\n\n        }).catch((error) => {\n          console.log(error)\n        })\n      },\n      async getFeatured(context) {\n        axios.get('api/featured').then((response) => {\n          context.commit(\"setFeatured\", response.data.data);\n        }).catch((error) => {\n          console.log(error)\n        })\n      },\n      async getProperties(context) {\n        axios.get('api/property?size=25&page=0').then((response) => {\n          context.commit(\"setProperty\", response.data.data);\n        }).catch((error) => {\n          console.log(error)\n        })\n      },\n      getModalList(context, data) {\n        context.commit(\"setModalList\", data);\n      }\n    },\n    getters: {\n      page_props: state => state.page_props,\n      featured: state => state.featured,\n      properties: state => state.properties,\n      modal_list: state => state.modal_list\n    }\n})\n  \ncreateApp(App)\n    .use(router)\n    .use(VueAxios, axios)\n    .use(store)\n    .component('font-awesome-icon', FontAwesomeIcon)\n    .mount('#app')\n"]}]}