;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,kBAAX,EAA+B,KAA/B;;AAEA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACH,OAbD,EAaG,KAbH,CAaS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,IAAzB;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,aAAjB,GAAiC,gDAAgD,KAAjF;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AAEA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,OApBD;AAqBH,KAtEI;AAuEL,IAAA,aAvEK,yBAuES,KAvET,EAuEgB;AAAA;;AACjB,UAAM,OAAM,GAAI,KAAK,SAAL,CAAe,eAAf,EAAgC,KAAhC,CAAhB;AACA,UAAM,cAAa,GAAI,EAAvB;AAEA,MAAA,MAAM,CAAC,OAAP,CAAe,OAAf,EAAwB,OAAxB,CAAgC,UAAA,OAAM,EAAK;AACvC,QAAA,cAAc,CAAC,IAAf,CAAoB;AAChB,UAAA,KAAK,EAAE,OAAO,CAAC,CAAD,CADE;AAEhB,UAAA,IAAI,EAAE,OAAO,CAAC,CAAD,CAFG;AAGhB,UAAA,SAAS,EAAE,eAHK;AAIhB,UAAA,SAAS,EAAE,IAJK;AAKhB,UAAA,IAAI,EAAE,MALU;AAMhB,UAAA,KAAK,EAAE,KANS;AAOhB,UAAA,MAAM,EAAE;AAPQ,SAApB;AASH,OAVD;AAYA,WAAK,KAAL,CAAW,IAAX,CAAgB,4BAAhB,EAA8C,cAA9C,EAA8D,IAA9D,CAAmE,UAAC,QAAD,EAAc;AAC7E,QAAA,MAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,KAAzB;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,IAA3B;AAEA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ,EAL6E,CAM7E;;AACA,YAAM,KAAI,GAAI,MAAM,CAAC,MAAP,CAAc,MAAI,CAAC,SAAnB,CAAd;AACA,QAAA,KAAK,CAAC,eAAD,CAAL,CAAuB,MAAvB,CAA8B,KAA9B,EAAqC,CAArC,EAR6E,CAU7E;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,kBAAX,EAA+B,KAA/B;AACH,OAZD,EAYG,KAZH,CAYS,UAAC,KAAD,EAAW;AAChB,QAAA,MAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,KAAjB,GAAyB,IAAzB;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,aAAjB,GAAiC,+CAA+C,KAAhF;AACA,QAAA,MAAI,CAAC,WAAL,CAAiB,OAAjB,GAA2B,KAA3B;AACH,OAjBD;AAkBJ;AAzGK;AA1CE,CAAf","sourcesContent":["<template>\n    <div class=\"container p-4\">\n        <div class=\"position-absolute cog p-4\">\n            <button class=\"btn btn-dark bg-gradient btn-sm\" data-bs-toggle=\"modal\" data-bs-target=\"#servicesProps\">\n                <font-awesome-icon :icon=\"['fas', 'cogs']\"></font-awesome-icon>\n            </button>\n        </div>\n\n        <div class=\"row ps-4 pe-4\">\n            <div class=\"col-md-12\">\n                <h2 class=\"text-dark fw-bold\">{{ prop_data.title }}</h2>\n                <p>{{ prop_data.tagline }}</p>\n            </div>\n        </div>\n        \n        <div class=\"row mt-4 mb-4 service-item\" v-for=\"({title, tagline, align, description}, index) in prop_data['service-items']\" :key=\"index\">  \n            <!-- first item -->\n            <div class=\"col-md-3 d-flex\" v-if=\"align == 'left'\">\n                <img class=\"mx-auto\" style=\"height: 30vh\" :src=\"require('../../../assets/img/investment.png')\" alt=\"\">\n            </div>   \n                                            \n            <div class=\"col-md-8 col-sm-12 p-4\">\n                <p class=\"text-danger text-uppercase mb-1 title\">{{ title }}</p>\n\n                <h6 class=\"text-dark tagline\">\n                    {{ tagline }}\n                </h6> \n                \n                <p v-if=\"description\" class =\"mb-3\" style=\"font-size: 16px;\">\n                    {{ description }}\n                </p>\n            </div>  \n\n            <!-- first item -->\n            <div class=\"col-md-3 col-sm-12 d-flex mt-4 order-top\" v-if=\"align == 'right'\">\n                <img class=\"mx-auto\" style=\"height: 30vh\" :src=\"require('../../../assets/img/investment.png')\" alt=\"\">\n            </div>   \n        </div>\n    </div>\n\n    <div class=\"modal fade\" id=\"servicesProps\" tabindex=\"-1\" aria-labelledby=\"servicesPropsLabel\" aria-hidden=\"true\">\n        <div class=\"modal-dialog\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header pb-0 border-bottom-0\">\n                    <span class=\"d-block w-100\">\n                        <h5 class=\"text-dark text-center\">Edit Services Section</h5>\n                    </span>\n                </div>\n                \n                <div class=\"modal-body container\">\n                    <div class=\"row\">\n\n                        <status v-model:status_data=status_data></status>\n\n                        <form action=\"#\" @submit.prevent=\"submit(form_data, [false, ''], 'services')\" v-show=\"screen == 1\">\n                            <div class=\"row mb-4\">\n                                <label for=\"title\" class=\"fw-bold mb-2\">Title</label>\n                                <div class=\"input-group\">\n                                    <input id=\"title\" type=\"text\" class=\"form-control\" v-model=\"form_data.title\" required>\n                                </div>\n                                <small class=\"text-muted\">Enter your title.</small>\n                            </div>\n\n                            <div class=\"row mb-4\">\n                                <label for=\"tagline\" class=\"fw-bold mb-2\">Tagline</label>\n                                <div class=\"input-group\">\n                                    <input id=\"tagline\" type=\"text\" class=\"form-control\" v-model=\"form_data.tagline\" required> \n                                </div>\n                                <small class=\"text-muted\">Enter your tagline.</small>\n                            </div>\n\n                            <div class=\"row mb-4\">\n                                <label for=\"image\" class=\"fw-bold mb-2\">Background color</label>\n\n                                <div class=\"input-group\">\n                                    <select id=\"background_color\" class=\"form-select\" v-model=\"form_data.background_color\" required>\n                                        <option></option>\n                                        <option value=\"white\">White</option>\n                                        <option value=\"light\">Light</option>\n                                        <option value=\"dark\">Dark</option>\n                                        <option value=\"burgundy\">Burgundy</option>\n                                    </select>\n                                </div>\n                                <small class=\"text-muted\">Enter the background color.</small>\n                            </div>\n\n                            <div class=\"row mb-4 mt-2\">\n                                <div class=\"col-md-12 mt-2 d-flex\">\n                                    <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                        Submit\n                                    </button>\n\n                                    <button type=\"button\" class=\"btn bg-secondary text-white w-25\" @click=\"changeScreen(2)\">\n                                        <font-awesome-icon :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                                    </button>\n                                </div>\n                            </div>                              \n                        </form>\n\n                        <form action=\"#\"  @submit.prevent=\"submit(services, [false, ''], 'service-items', true)\" v-show=\"screen == 2\">\n                            <p class=\"text-burgundy fw-bold\">Add a Service entry</p>\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Title</label>\n\n                                <div class=\"input-group\">\n                                    <input type=\"text\" class=\"form-control\" v-model=\"services.title\" required>\n                                </div>\n                                <small class=\"text-muted\">Enter your title.</small>\n                            </div>  \n\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Tagline</label>\n\n                                <div class=\"input-group\">\n                                    <textarea rows=\"2\" class=\"form-control\" v-model=\"services.tagline\" required> </textarea>\n                                </div>\n                                <small class=\"text-muted\">Enter your tagline.</small>\n                            </div>\n\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Description</label>\n\n                                <div class=\"input-group\">\n                                    <textarea type=\"text\" class=\"form-control\" v-model=\"services.description\" rows=\"4\"> </textarea>\n                                </div>\n                                <small class=\"text-muted\">Enter your description.</small>\n                            </div> \n\n                            <div class=\"row mb-4\">\n                                <label for=\"side\" class=\"fw-bold mb-2\">Align Image in Entry</label>\n\n                                <div class=\"input-group\">\n                                    <select name=\"side\" id=\"side\" class=\"form-select\" v-model=\"services.align\">\n                                        <option></option>\n                                        <option value=\"right\">Right</option>\n                                        <option value=\"left\">Left</option>\n                                    </select>\n                                </div>\n                                <small class=\"text-muted\">Upload image for service.</small>\n                            </div>                                     \n\n                            <div class=\"row mb-4 mt-2\">\n                                <div class=\"col-md-12 mt-2 d-flex\">\n                                    <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                        Submit\n                                    </button>\n\n                                    <span class=\"w-25 d-flex\">\n                                        <button type=\"button\" class=\"btn bg-secondary text-white me-2 w-50\" @click=\"changeScreen(1)\">\n                                            <font-awesome-icon :icon=\"['fas', 'angle-left']\"></font-awesome-icon>\n                                        </button>\n\n                                        <button type=\"button\" class=\"btn bg-secondary text-white w-50\" @click=\"changeScreen(3)\">\n                                            <font-awesome-icon :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                                        </button>\n                                    </span>\n                                </div>\n                            </div>                                                                    \n                        </form>\n\n                        <div v-show=\"screen == 3\">\n                            <p class=\"text-burgundy fw-bold\">Delete a Service entry</p>\n\n                            <div class=\"row\" v-for=\"({title, tagline}, index) in prop_data['service-items']\" :key=\"index\">\n\n                                <div class=\"col-md-12 d-flex\">\n                                    <div class=\"d-block me-4\">\n                                        <p class=\"mb-0 text-dark\">&bull; {{ title }}</p>\n                                        <p class=\"text-muted mt-1\">{{ tagline }}</p>                                        \n                                    </div>\n                                    <div class=\"d-block ms-auto\">\n                                        <p class=\"mb-0\" style=\"cursor: pointer\" @click=\"remove(prop_data['service-items'], index, 'service-items')\">\n                                            <font-awesome-icon class=\"text-burgundy\" :icon=\"['fas', 'trash-alt']\"></font-awesome-icon>\n                                        </p>\n                                    </div>\n                                </div>\n\n                            </div>\n\n                            <div class=\"col-md-12 mt-2 d-flex\">\n                                <button type=\"button\" class=\"btn bg-secondary text-white w-25 me-2\" @click=\"changeScreen(2)\">\n                                    <font-awesome-icon :icon=\"['fas', 'angle-left']\"></font-awesome-icon>\n                                </button>\n\n                                <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                    Submit\n                                </button>\n\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style lang=\"scss\" scoped>\n    @import '../../../assets/scss/helpers/_breakpoints.scss';\n    .title {\n        font-weight: 700; \n        letter-spacing: 1.3px;\n    }\n\n    .tagline {\n        font-size: 1.6rem; \n        font-weight: 500;\n    }\n\n    .order-top {\n        @include media('<=tablet') {\n            order: -1!important;\n        }\n    }\n\n    .service-item {\n        padding: 0 4.5rem;\n         @include media('<=tablet') {\n            padding: 0 1.5rem;\n        }\n    }\n\n</style>\n\n<script>\n    import Status from '../../Status';  \n    import handleProps from '../../../mixins/handleProps';\n    \n    export default {\n        name: \"Services\",\n        mixins: [handleProps],\n        components: {\n            Status\n        },\n        props: {\n            prop_data: {\n                required: true,\n                type: Object\n            },\n            page: {\n                required: true, \n                type: String\n            }             \n        },\n        data() {\n            return {\n                form_data: {\n                    title: this.prop_data.title,\n                    tagline: this.prop_data.tagline,\n                    background_color: this.prop_data.background_color\n                },\n                services: {\n                    title: '',\n                    tagline: '',\n                    description: '',\n                    align: ''\n                },\n                status_data: {\n                    success: false,\n                    success_message: 'Form Submission was successful!',\n                    error: false,\n                    error_message: 'There are some errors.',\n                    loading: false\n                }, \n                screen: 1\n            }\n        },\n        mounted() {\n            console.log(this.prop_data)\n        },\n        methods: {\n            changeScreen(value) {\n                this.screen = value\n            },\n            submitProps() {\n                var post_form_data = []\n\n                Object.entries(this.form_data).forEach(element => {\n                    if(this.prop_data[element[0]] != element[1]) {\n                        post_form_data.push({\n                            value: element[1],\n                            prop: element[0],\n                            component: 'services',\n                            container: false,\n                            page: 'Home',\n                            child: false,\n                            parent: \"\"\n                        })\n                    }\n                });\n\n                this.axios.post('api/prop', post_form_data).then(() => {\n                    this.status_data.loading = false\n                    this.status_data.error = false\n                    this.status_data.success = true\n                }).catch((error) => {\n                    this.status_data.loading = false\n                    this.status_data.error = true\n                    this.status_data.error_message = 'Section data was not successfully updated!' + error\n                    this.status_data.success = false\n                })\n            },\n            submitServices() {\n                var post_form_data = []\n\n                Object.entries(this.services).forEach(element => { \n                    if(element[1] != '') {\n                        post_form_data.push({\n                            value: element[1],\n                            prop: element[0],\n                            component: 'service-items',\n                            container: true,\n                            page: 'Home',\n                            child: false,\n                            parent: \"\"\n                        })\n                    }\n                });\n\n                this.axios.post('api/prop', post_form_data).then((response) => {\n                    this.status_data.loading = false\n                    this.status_data.error = false\n                    this.status_data.success = true\n\n                    // add service \n                    const props = Object.create(this.prop_data)\n                    props['service-items'].push(this.services)\n\n                    // update prop data\n                    this.$emit('update:prop_data', props)  \n\n                    console.log(response)\n                }).catch((error) => {\n                    this.status_data.loading = false\n                    this.status_data.error = true\n                    this.status_data.error_message = 'Section data was not successfully updated! ' + error\n                    this.status_data.success = false\n\n                    console.log(error)\n                })\n            },\n            deleteService(index) {\n                const service = this.prop_data['service-items'][index]\n                const post_form_data = []\n\n                Object.entries(service).forEach(element => {\n                    post_form_data.push({\n                        value: element[1],\n                        prop: element[0],\n                        component: 'service-items',\n                        container: true,\n                        page: 'Home',\n                        child: false,\n                        parent: \"\"\n                    })\n                });\n\n                this.axios.post('api/component-props/delete', post_form_data).then((response) => {\n                    this.status_data.loading = false\n                    this.status_data.error = false\n                    this.status_data.success = true\n                    \n                    console.log(response)\n                    // unset service that was deleted\n                    const props = Object.create(this.prop_data)\n                    props['service-items'].splice(index, 1)\n\n                    // update prop data\n                    this.$emit('update:prop_data', props)\n                }).catch((error) => {\n                    this.status_data.loading = false\n                    this.status_data.error = true\n                    this.status_data.error_message = 'Section data was not successfully updated!' + error\n                    this.status_data.success = false\n                })\n            }\n        }\n    }\n</script>"],"sourceRoot":""}]}