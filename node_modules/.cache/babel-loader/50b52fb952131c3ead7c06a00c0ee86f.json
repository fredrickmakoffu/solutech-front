lass=\"col-lg-6 col-md-8 mx-auto\">\n            <h1 class=\"fw-light fw-bold text-dark\">Properties you'd love to <span class=\"text-burgundy fw-bold\" style=\"text-decoration: underline; text-underline-offset: 8px\"> {{ sale_type }}</span></h1>\n          </div>\n\n          <form action=\"#\" @submit.prevent=\"filterProperties()\" class=\"row d-flex mt-4 p-4\">\n            <status v-model:status_data=status_data></status>\n\n            <div class=\"col-md-2 col-sm-12 mt-4\">\n              <div class=\"form-row\">\n                <div class=\"input-group\">\n                  <select class=\"form-select text-secondary\" v-model=\"form_data.address\">\n                    <option value=\"null\" selected>Choose Locations</option>\n                    <option value=\"Nairobi\">Nairobi</option>\n                    <option value=\"Mombasa\">Mombasa</option>\n                    <option value=\"Thika\">Thika</option>\n                  </select>\n\n                  <span v-show=\"form_data.address != 'null' && form_data.address != null\" class=\"input-group-text bg-transparent border-start-0\" style=\"border: 1px solid #eee\">\n                      <font-awesome-icon class=\"text-info\" id=\"eye\" :icon=\"['fa', 'check-circle']\" />\n                  </span>                  \n                </div>\n              </div>\n            </div>\n\n            <div class=\"col-md-2 col-sm-12 mt-4\">\n              <div class=\"form-row\">\n                <div class=\"input-group\">\n                  <select class=\"form-select text-secondary\" v-model=\"form_data.type\">\n                    <option value=\"null\" selected>Choose Property type</option>\n                    <option value=\"Mansionettes\">Mansionettes</option>\n                    <option value=\"Bungalows\">Bungalow</option>\n                    <option value=\"Apartments\">Apartments</option>\n                    <option value=\"Villa\">Villa</option>\n                  </select>\n\n                  <span v-show=\"form_data.type != '' && form_data.type != null\" class=\"input-group-text bg-transparent border-left-0\" style=\"border: 1px solid #eee\">\n                      <font-awesome-icon class=\"text-info\" id=\"eye\" :icon=\"['fa', 'check-circle']\" />\n                  </span> \n                </div>\n              </div>\n            </div>                  \n\n            <div class=\"col-md-2 col-sm-12 mt-4\">\n              <div class=\"form-row\">\n                <div class=\"input-group\">\n                  <select class=\"form-select text-secondary\" v-model=\"form_data.bedrooms\">\n                    <option value=\"null\" selected>Choose no. Bedrooms</option>\n                    <option value=\"1\">Studio</option>\n                    <option value=\"2\">2 bedrooms</option>\n                    <option value=\"3\">3 bedrooms</option>\n                    <option value=\"4\">4 bedrooms</option>\n                  </select>\n\n                  <span v-show=\"form_data.bedrooms != '' && form_data.bedrooms != null\" class=\"input-group-text bg-transparent border-left-0\" style=\"border: 1px solid #eee\">\n                      <font-awesome-icon class=\"text-info\" id=\"eye\" :icon=\"['fa', 'check-circle']\" />\n                  </span>                   \n                </div>\n              </div>\n            </div>                  \n\n            <div class=\"col-md-2 col-sm-12 mt-4\">\n              <div class=\"form-row\">\n                <div class=\"input-group\">\n                  <input type=\"text\" class=\"form-control text-secondary\" v-model=\"form_data.min_price\" placeholder=\"Set minimum price.\">\n                  \n                  <span v-show=\"form_data.min_price != '' && form_data.min_price != null\" class=\"input-group-text bg-transparent border-left-0\" style=\"border: 1px solid #eee\">\n                      <font-awesome-icon class=\"text-info\" id=\"eye\" :icon=\"['fa', 'check-circle']\" />\n                  </span>                     \n                </div>\n              </div>\n            </div>\n\n            <div class=\"col-md-2 col-sm-12 mt-4\">\n              <div class=\"form-row\">\n                <div class=\"input-group\">\n                  <input type=\"text\" class=\"form-control text-secondary\" aria-label=\"max_price\" v-model=\"form_data.max_price\" placeholder=\"Set maximum price.\">\n\n                  <span v-show=\"form_data.max_price != '' && form_data.max_price != null\" class=\"input-group-text bg-transparent border-left-0\" style=\"border: 1px solid #eee\">\n                      <font-awesome-icon class=\"text-info\" id=\"eye\" :icon=\"['fa', 'check-circle']\" />\n                  </span>                   \n                </div>\n              </div>\n            </div>\n\n            <div class=\"col-md-2 col-sm-12 mt-4 d-flex\">\n              <button class=\"btn bg-burgundy text-white w-100\" style=\"line-height: 3\">\n                Search <font-awesome-icon :icon=\"['fas', 'search']\"></font-awesome-icon>\n              </button>\n            </div>\n          </form>\n        </div>\n\n        <div class=\"d-flex\">\n          <div class=\"bg-light mx-auto rounded w-50 mt-2\" style=\"padding-top: 2px\"></div>\n        </div>\n      </section>\n\n      <section class=\"property mb-4\">\n        <div class=\"row p-4\">\n          <div class=\"d-flex mt-4 pe-4\">  \n            <p class=\"ms-auto text-secondary fw-bold mb-0\">Total {{ totals }} properties</p>\n          </div>\n          \n          <div class=\"row\" v-if=\"properties.length > 0\">\n            <div class=\"col-md-4 col-sm-12 mt-4\" v-for=\"({name, address, bedrooms, image_url, bathrooms, sale_type, id, index}) in properties\" :key=\"index\">\n                <div class=\"card rounded\" style=\"box-shadow: 0 4px 8px rgb(0 0 0 / 3%); border-color: #eee;\">\n                    <img v-if=\"image_url != null\" :src=\"axios.defaults.baseURL + '/' + image_url\" alt=\"\">\n\n                    <div class=\"card-body\">\n                        <router-link class=\"text-decoration-none\" :to=\"{ name: 'Property', params: {property_id: id}}\">\n                            <h6 class=\"text-dark fw-bold\">{{ name }}</h6>\n                        </router-link>\n                        <p class=\"text-muted\">{{ bedrooms }} bedrooms, {{ bathrooms }} bathrooms</p>\n\n                        <div class=\"d-flex mt-4\">\n                            <p>&bull; {{ address }}</p>\n                            <p :class=\"['ms-auto fw-bold text-capitalize text-burgundy', \n                            {'text-burgundy': sale_type == 'build'}, {'text-success': sale_type == 'buy'}\n                            ]\">&bull; {{ sale_type }}</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n          </div>  \n\n          <div v-if=\"properties.length == 0\">\n            <no-data :message=\"'No properties listed.'\"></no-data>\n          </div> \n        </div>\n\n        <div class=\"row\">\n          <div class=\"col-md-12 d-flex\">\n            <div class=\"mx-auto\">\n\n              <button id=\"back\" class=\"btn btn-dark mt-4 me-2\" v-if=\"page > 0\"  @click=\"previous()\">\n                <p class=\"text-uppercase fw-bold mb-0\">\n                  <font-awesome-icon :icon=\"['fas', 'angle-left']\"></font-awesome-icon>\n                </p>\n              </button>\n\n              <button v-if=\"(((page) * size) + size) <= totals\" id=\"submit\" class=\"btn btn-dark border-0 me-3 mt-4 ps-4 pe-4\" @click=\"next()\">\n                <p class=\"text-uppercase fw-bold mb-0\" style=\"letter-spacing: 4px\">Load More</p>\n              </button>\n            </div>\n          </div>\n        </div>  \n      </section>\n\n      <section id=\"tagline\" class=\"text-center container mb-4\">\n        <h1 class=\"text-dark fw-bold\">Let us know if you need something more <span class=\"text-burgundy\" style=\"text-decoration: underline; text-underline-offset: 8px\">tailored</span></h1>\n\n        <router-link :to=\"{name: 'Contacts'}\" class=\"btn bg-burgundy text-white btn-lg mt-4\">\n          Contact Us <font-awesome-icon class=\"ms-2\" :icon=\"['fas', 'comment']\"></font-awesome-icon>\n        </router-link>\n      </section>\n\n      <main-footer v-if=\"page_props.footer\" :page=\"'Home'\" v-model:prop_data=\"page_props.footer\"></main-footer>\n    </div>\n  </div>\n</template>\n\n<style lang=\"scss\" scoped>\n  @import '../../assets/scss/abstracts/_variables.scss';\n  .body {\n    padding-left: 0!important;\n  }\n\n  .navbar-border-bottom {\n    border-bottom: 1px solid #fafafa !important;\n    box-shadow: 0 4px 8px rgb(0 0 0 / 3%);\n  }\n\n  .property {\n    padding-top: 1rem;\n    padding-bottom: 1rem;\n  }\n  \n  #tagline {\n    padding-top: 4rem;\n    padding-bottom: 4rem;\n  }\n\n  .form-select, .form-control {\n    line-height: 3;\n    border: 1px solid #eee;\n  }\n\n\n  .form-select option{\n    color: $dark-color;\n  }\n\n  .form-select option:first-child{\n    color: $secondary-secondary-color;\n  }\n\n  .card {\n    border: 1px solid #eee;\n  }\n\n  .image-backdrop {\n    position: absolute;\n    display: flex;\n    width: 100%;\n    height: 100%;\n    \n    &:hover {\n      z-index: 5;\n      background: #000;\n      opacity: 0.4;\n    }\n  }\n  .circle {\n    width: 60px;\n    height: 65px;\n    align-items: center;\n    justify-content: center;\n    border-radius: 50%;\n    font-size: 1rem;\n    z-index: 6;\n    position: absolute;\n    right: 0;\n    top: 50%;\n    bottom: 50%;\n    cursor: pointer;\n  }\n\n</style>\n\n<script>\n  import Breadcrumbs from '../../components/AutoBreadcrumbs'\n  import Navbar from '../../components/Custom/PropertyNavbar'\n  import MainFooter from '../../components/Footer'\n  import Status from '../../components/Status'\n  import NoData from '../../components/NoData'\n\n  export default {\n    name: 'Home',\n    props: ['sale_type'],\n    components: {\n      Navbar,\n      Breadcrumbs,\n      MainFooter,\n      Status,\n      NoData\n    }, \n    data() {\n      return {\n        title: {status: true, title: 'Your Data', actions: true},\n        screen: 0,\n        breadcrumbs_data: [\n          {title: 'Home', link: '', active: false},\n          {title: 'Properties', active: true},\n        ],\n        properties: {},\n        size: 12,\n        page: 0,\n        totals: 0,\n        form_data: {\n          min_price: null,\n          max_price: null,\n          address: null,\n          type: null,\n          bedrooms: null,\n          sale_type: this.sale_type\n        },\n        status_data: {\n            \"success\": false,\n            \"success_message\": 'Filter Successful!',\n            \"error\": false,\n            \"error_message\": 'There were some errors.',\n            \"loading\": false\n        },        \n      } \n    },\n    watch: {\n      sale_type: function() {\n        this.getProperties()\n      }\n    },\n    computed: {\n      page_props () {\n        return this.$store.state.page_props\n      }\n    }, \n    mounted() {\n      if(this.page_props.length == 0) {\n        this.$store.dispatch(\"getProps\");\n      } \n            \n      this.getProperties()\n    },\n    methods: {\n      changeScreen(value) {\n        this.screen = value\n      },\n      getProperties() {\n        this.status_data.loading = true\n\n        this.axios.get('api/property/filter/sale_type?sale_type=' + this.sale_type).then((response) => {\n          this.status_data.loading = false\n          this.properties = response.data.data\n          alert(this.properties.length)\n          this.totals = response.data.totals\n        }).catch((error) => {\n          this.properties = []\n          this.status_data.loading = false\n          console.log(error)\n        })\n      },\n      filterProperties() {\n        this.status_data.loading = true\n\n        this.axios.post('api/filter-property?size=' + this.size + '&page=' + this.page + '', this.form_data).then((response) => {\n          this.status_data.loading = false\n          this.properties = response.data.data\n          this.totals = response.data.totals\n          document.querySelector('.property').scrollIntoView({behavior: \"smooth\"})\n        }).catch((error) => {\n          this.status_data.loading = false\n          document.querySelector('.property').scrollIntoView({behavior: \"smooth\"}) \n          console.log(error)\n        })\n      },\n      next() {\n        if((this.page*this.size) < this.totals) {\n          this.page = this.page + 1\n          this.filterProperties()\n        }\n      },\n      previous() {\n        if(this.page >= 0) {\n          this.page = this.page - 1\n          this.filterProperties()\n        }\n      }      \n    }\n  }\n</script>\n"],"sourceRoot":""}]}