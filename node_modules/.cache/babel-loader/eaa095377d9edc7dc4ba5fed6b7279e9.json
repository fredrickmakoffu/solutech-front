AAA,aAAE,QAAA,CAAA,YAAA,CAAY,CAAZ,CAAF;AAAA,KAAA;AAE5D,GAFT,E,CACI,YAAA,CAAsE,4BAAtE,EAAsE;AAAlD,IAAA,IAAI,EAAE,CAAA,KAAA,EAAA,aAAA;AAA4C,GAAtE,C,CADJ,CAGG,CARP,CASE,CAdN,CAeE,CAhBN,C,CAtCJ,E,GAAA,C,YAAiG,KAAA,CAAA,MAAA,IAAM,C,GAuFrG,E,gBA9BF,YAAA,CA6BM,KA7BN,EA6BM,IA7BN,EA6BM,CA5BF,WA4BE,G,kBA1BF,YAAA,CAcM,SAdN,EAcM,IAdN,EAcM,WAAA,CAd+C,MAAA,CAAA,SAAA,CAAS,eAAT,CAc/C,EAdwD,iBAAnB,KAAmB,EAAd;AAAA,QAAtB,KAAsB,SAAtB,KAAsB;AAAA,QAAf,OAAe,SAAf,OAAe;yBAAhD,YAAA,CAcM,KAdN,EAcM;AAdD,MAAA,KAAK,EAAC,KAcL;AAd4E,MAAA,GAAG,EAAE;AAcjF,KAdN,E,CAEI,YAAA,CAUM,KAVN,EAAA,WAAA,EAUM,CATF,YAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFF,YAAA,CAAgD,GAAhD,EAAA,WAAA,EAA0B,OAAO,gBAAA,CAAG,KAAH,CAAjC,EAAyC,CAAzC,CAEE,EADF,YAAA,CAA4C,GAA5C,EAAA,WAAA,EAA4C,gBAAA,CAAd,OAAc,CAA5C,EAAqC,CAArC,CACE,CAHN,CASE,EALF,YAAA,CAIM,KAJN,EAAA,WAAA,EAIM,CAHF,YAAA,CAEI,GAFJ,EAEI;AAFD,MAAA,KAAK,EAAC,MAEL;AAFY,MAAA,KAAuB,EAAvB;AAAA,kBAAA;AAAA,OAEZ;AAFqC,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,IAAA,CAAA,MAAA,CAAO,MAAA,CAAA,SAAA,CAAS,eAAT,CAAP,EAAmC,KAAnC,EAAwC,eAAxC,CAAF;AAAA;AAE1C,KAFJ,E,CACI,YAAA,CAA0F,4BAA1F,EAA0F;AAAvE,MAAA,KAAK,EAAC,eAAiE;AAAhD,MAAA,IAAI,EAAE,CAAA,KAAA,EAAA,WAAA;AAA0C,KAA1F,C,CADJ,E,CAAA,E,WAAA,CAGE,CAJN,CAKE,CAVN,C,CAFJ,C;GAcM,CAdN,E,GAAA,CA0BE,GAVF,YAAA,CASM,KATN,EAAA,WAAA,EASM,CARF,YAAA,CAES,QAFT,EAES;AAFD,IAAA,IAAI,EAAC,QAEJ;AAFa,IAAA,KAAK,EAAC,uCAEnB;AAF4D,IAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,YAAA,CAAY,CAAZ,CAAF;AAAA,KAAA;AAEjE,GAFT,E,CACI,YAAA,CAAqE,4BAArE,EAAqE;AAAjD,IAAA,IAAI,EAAE,CAAA,KAAA,EAAA,YAAA;AAA2C,GAArE,C,CADJ,CAQE,EAJF,WAIE,CATN,CAUE,CA7BN,E,GAAA,C,YAAa,KAAA,CAAA,MAAA,IAAM,C,GA8BjB,CAxIN,CAyIE,CA1IN,CA2IE,CAlJN,CAmJE,CApJN,CAqJE,CAtJN,C","sourcesContent":["<template>\n    <div class=\"container p-4\">\n        <div class=\"position-absolute cog p-4\" v-if=\"if_logged_in\">\n            <button class=\"btn btn-dark bg-gradient btn-sm\" data-bs-toggle=\"modal\" data-bs-target=\"#servicesProps\">\n                <font-awesome-icon :icon=\"['fas', 'cogs']\"></font-awesome-icon>\n            </button>\n        </div>\n\n        <div class=\"row ps-4 pe-4\">\n            <div class=\"col-md-12 text-center mb-4\">\n                <h2 class=\"text-dark fw-bold blue-gradient-text\">{{ prop_data.title }}</h2>\n                <p style=\"font-size: 15px; letter-spacing: 0.4px;\">{{ prop_data.tagline }}</p>\n            </div>\n        </div>\n        \n        <div class=\"row mt-4 mb-4 service-item\" v-for=\"({title, tagline, description, image}, index) in prop_data['service-items']\" :key=\"index\">  \n            <!-- first item -->\n            <div class=\"col-md-3 d-flex\" v-if=\"index%2 == 0\">\n                <img class=\"mx-auto\" style=\"height: 30vh\" :src=\"image\" alt=\"\">\n            </div>   \n                                            \n            <div class=\"col-md-8 col-sm-12 p-4\">\n                <p class=\"text-danger text-uppercase mb-0 title\">{{ title }}</p>\n\n                <h5 class=\"text-dark tagline\">\n                    {{ tagline }}\n                </h5> \n                \n                <p v-if=\"description\" class =\"mb-3 mt-2\" style=\"font-size: 17px;\">\n                    {{ description }}\n                </p>\n            </div>  \n\n            <!-- first item -->\n            <div class=\"col-md-3 col-sm-12 d-flex mt-4 order-top\" v-if=\"index%2 == 1\">\n                <img class=\"mx-auto\" style=\"height: 30vh\" :src=\"image\" alt=\"\">\n            </div>   \n        </div>\n    </div>\n\n    <div class=\"modal fade\" id=\"servicesProps\" tabindex=\"-1\" aria-labelledby=\"servicesPropsLabel\" aria-hidden=\"true\">\n        <div class=\"modal-dialog\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header pb-0 border-bottom-0\">\n                    <span class=\"d-block w-100\">\n                        <h5 class=\"text-dark text-center\">Edit Services Section</h5>\n                    </span>\n                </div>\n                \n                <div class=\"modal-body container\">\n                    <div class=\"row\">\n\n                        <status v-model:status_data=status_data></status>\n\n                        <form action=\"#\" @submit.prevent=\"submit(form_data, [false, ''], 'services')\" v-show=\"screen == 1\">\n                            <div class=\"row mb-4\">\n                                <label for=\"title\" class=\"fw-bold mb-2\">Title</label>\n                                <div class=\"input-group\">\n                                    <input id=\"title\" type=\"text\" class=\"form-control\" v-model=\"form_data.title\" required>\n                                </div>\n                                <small class=\"text-muted\">Enter your title.</small>\n                            </div>\n\n                            <div class=\"row mb-4\">\n                                <label for=\"tagline\" class=\"fw-bold mb-2\">Tagline</label>\n                                <div class=\"input-group\">\n                                    <input id=\"tagline\" type=\"text\" class=\"form-control\" v-model=\"form_data.tagline\" required> \n                                </div>\n                                <small class=\"text-muted\">Enter your tagline.</small>\n                            </div>\n\n                            <div class=\"row mb-4\">\n                                <label for=\"image\" class=\"fw-bold mb-2\">Background color</label>\n\n                                <div class=\"input-group\">\n                                    <select id=\"background_color\" class=\"form-select\" v-model=\"form_data.background_color\" required>\n                                        <option></option>\n                                        <option value=\"white\">White</option>\n                                        <option value=\"light\">Light</option>\n                                        <option value=\"dark\">Dark</option>\n                                        <option value=\"burgundy\">Burgundy</option>\n                                    </select>\n                                </div>\n                                <small class=\"text-muted\">Enter the background color.</small>\n                            </div>\n\n                            <div class=\"row mb-4 mt-2\">\n                                <div class=\"col-md-12 mt-2 d-flex\">\n                                    <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                        Submit\n                                    </button>\n\n                                    <button type=\"button\" class=\"btn bg-secondary text-white w-25\" @click=\"changeScreen(2)\">\n                                        <font-awesome-icon :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                                    </button>\n                                </div>\n                            </div>                              \n                        </form>\n\n                        <form action=\"#\"  @submit.prevent=\"submit(services, [false, ''], 'service-items', true)\" v-show=\"screen == 2\">\n                            <p class=\"text-burgundy fw-bold\">Add a Service entry</p>\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Title</label>\n\n                                <div class=\"input-group\">\n                                    <input type=\"text\" class=\"form-control\" v-model=\"services.title\" @change=\"ifUnique('title', $event.currentTarget)\" required>\n                                </div>\n                                <small class=\"text-muted\">Enter your title.</small>\n                            </div>  \n\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Tagline</label>\n\n                                <div class=\"input-group\">\n                                    <textarea rows=\"2\" class=\"form-control\" v-model=\"services.tagline\" @change=\"ifUnique('tagline', $event.currentTarget)\" required> </textarea>\n                                </div>\n                                <small class=\"text-muted\">Enter your tagline.</small>\n                            </div>\n\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Description</label>\n\n                                <div class=\"input-group\">\n                                    <textarea type=\"text\" class=\"form-control\" v-model=\"services.description\" @change=\"ifUnique('description', $event.currentTarget)\" rows=\"4\"> </textarea>\n                                </div>\n                                <small class=\"text-muted\">Enter your description.</small>\n                            </div> \n\n                            <div class=\"row mb-4\">\n                                <label class=\"fw-bold mb-2\">Image</label>\n\n                                <div class=\"input-group\">\n                                    <input type=\"file\" class=\"form-control\" @change=\"onImageChange($event, 'services')\">\n                                </div>\n                                <small class=\"text-muted\">Enter your description.</small>\n                            </div>\n\n                            <div class=\"row mb-4 mt-2\">\n                                <div class=\"col-md-12 mt-2 d-flex\">\n                                    <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                        Submit\n                                    </button>\n\n                                    <span class=\"w-25 d-flex\">\n                                        <button type=\"button\" class=\"btn bg-secondary text-white me-2 w-50\" @click=\"changeScreen(1)\">\n                                            <font-awesome-icon :icon=\"['fas', 'angle-left']\"></font-awesome-icon>\n                                        </button>\n\n                                        <button type=\"button\" class=\"btn bg-secondary text-white w-50\" @click=\"changeScreen(3)\">\n                                            <font-awesome-icon :icon=\"['fas', 'angle-right']\"></font-awesome-icon>\n                                        </button>\n                                    </span>\n                                </div>\n                            </div>                                                                    \n                        </form>\n\n                        <div v-show=\"screen == 3\">\n                            <p class=\"text-burgundy fw-bold\">Delete a Service entry</p>\n\n                            <div class=\"row\" v-for=\"({title, tagline}, index) in prop_data['service-items']\" :key=\"index\">\n\n                                <div class=\"col-md-12 d-flex\">\n                                    <div class=\"d-block me-4\">\n                                        <p class=\"mb-0 text-dark\">&bull; {{ title }}</p>\n                                        <p class=\"text-muted mt-1\">{{ tagline }}</p>                                        \n                                    </div>\n                                    <div class=\"d-block ms-auto\">\n                                        <p class=\"mb-0\" style=\"cursor: pointer\" @click=\"remove(prop_data['service-items'], index, 'service-items')\">\n                                            <font-awesome-icon class=\"text-burgundy\" :icon=\"['fas', 'trash-alt']\"></font-awesome-icon>\n                                        </p>\n                                    </div>\n                                </div>\n\n                            </div>\n\n                            <div class=\"col-md-12 mt-2 d-flex\">\n                                <button type=\"button\" class=\"btn bg-secondary text-white w-25 me-2\" @click=\"changeScreen(2)\">\n                                    <font-awesome-icon :icon=\"['fas', 'angle-left']\"></font-awesome-icon>\n                                </button>\n\n                                <button type=\"submit\" class=\"btn btn-dark w-75 me-2\">\n                                    Submit\n                                </button>\n\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style lang=\"scss\" scoped>\n    @import '../../../assets/scss/helpers/_breakpoints.scss';\n    .title {\n        font-weight: 700;\n        letter-spacing: 1.3px;\n        font-size: 13px;\n    }\n\n    .tagline {        \n        font-weight: 500;\n        letter-spacing: .3px;\n    }\n\n    .order-top {\n        @include media('<=tablet') {\n            order: -1!important;\n        }\n    }\n\n    .service-item {\n        padding: 0 4.5rem;\n         @include media('<=tablet') {\n            padding: 0 1.5rem;\n        }\n    }\n\n</style>\n\n<script>\n    import Status from '../../Status';  \n    import handleProps from '../../../mixins/handleProps';\n    import image from '../../../mixins/image';\n\n    export default {\n        name: \"Services\",\n        mixins: [handleProps, image],\n        components: {\n            Status\n        },\n        props: {\n            prop_data: {\n                required: true,\n                type: Object\n            },\n            page: {\n                required: true, \n                type: String\n            }             \n        },\n        data() {\n            return {\n                form_data: {\n                    title: this.prop_data.title,\n                    tagline: this.prop_data.tagline,\n                    background_color: this.prop_data.background_color\n                },\n                services: {\n                    title: '',\n                    tagline: '',\n                    description: '',\n                    image: ''\n                },\n                status_data: {\n                    success: false,\n                    success_message: 'Form Submission was successful!',\n                    error: false,\n                    error_message: 'There are some errors.',\n                    loading: false\n                }, \n                screen: 1,\n                if_logged_in: false\n            }\n        },\n        mounted() {\n            this.ifLoggedIn()\n        },\n        methods: {\n            changeScreen(value) {\n                this.screen = value\n            },\n            ifUnique(value, event) {\n                if(this.prop_data['service-items'] != null) {\n                    var current_event = null\n\n                    this.prop_data['service-items'].forEach(element => {\n                        if(element[value].toLowerCase() == this.services[value].toLowerCase()) {\n                            current_event = event.parentNode.nextElementSibling\n\n                            this.services[value] = ''\n\n                            current_event.innerHTML = 'This '+ value + ' is already taken. Please choose something unique.'\n\n                            event.parentNode.nextElementSibling.classList.remove('text-muted')\n\n                            event.parentNode.nextElementSibling.classList.add('text-burgundy')\n                        }\n                    });\n\n                    if(current_event == null && event.parentNode.nextElementSibling.classList.contains('text-burgundy')) {\n\n                        event.parentNode.nextElementSibling.innerHTML = 'Nice!'\n\n                        event.parentNode.nextElementSibling.classList.remove('text-burgundy')\n\n                        event.parentNode.nextElementSibling.classList.add('text-primary')\n                    }\n                }\n            },\n            ifLoggedIn() {\n                if(localStorage.getItem('user') != null) {\n                    this.if_logged_in = true\n                } else {\n                    this.if_logged_in = false\n                }\n            }                      \n        }\n    }\n</script>"],"sourceRoot":""}]}