AFK;AAGX,QAAA,OAAO,EAAE;AAHE,OAAf;AAKF;AAjNO;AATE,CAAf","sourcesContent":["<template>\n    <!-- Aggregates -->\n    <div class=\"row mt-4 gx-2\">\n        <div class=\"col-lg-3 col-md-6 col-sm-6 mb-2\">\n            <div class=\"card stats primary\">    \n                <div class=\"card-body\">\n                    <div class=\"d-flex\">\n                        <span>\n                            <h4 class=\"fw-bold\">{{ formatNumber('2540')}}</h4>\n                            <p class=\"card-icon-title\">Clients</p>\n                        </span>\n                        \n                        <font-awesome-icon class=\"icon card-icon ms-auto\" :icon=\"['fa', 'users']\" /> \n                    </div>  \n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-lg-3 col-md-6 col-sm-6 mb-2\">\n            <div class=\"card stats success\">\n                <div class=\"card-body\">\n                    <div class=\"d-flex\">\n                        <span>\n                            <h4 class=\"fw-bold\">$ {{ formatNumber('125750')}}</h4>\n                            <p class=\"card-icon-title\">Active Accounts</p>\n                        </span>\n                        \n                        <font-awesome-icon class=\"icon card-icon ms-auto\" :icon=\"['fa', 'dollar-sign']\" /> \n                    </div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-lg-3 col-md-6 col-sm-6 mb-2\">\n            <div class=\"card stats info \">\n                <div class=\"card-body\">\n                    <div class=\"d-flex\">\n                        <span>\n                            <h4 class=\"fw-bold\">$ {{ formatNumber('5000')}}</h4>\n                            <p class=\"card-icon-title\">Profit Earned</p>\n                        </span>\n                        \n                        <font-awesome-icon class=\"icon card-icon ms-auto\" :icon=\"['fa', 'exclamation-triangle']\" /> \n                    </div>\n                </div>\n            </div>\n        </div>\n        \n        <div class=\"col-lg-3 col-md-6 col-sm-6 mb-2\">\n            <div class=\"card stats danger\">\n                <div class=\"card-body\">\n                    <div class=\"d-flex\">\n                        <span>\n                            <h4 class=\"fw-bold\">$ {{ formatNumber('600')}}</h4>\n                            <p class=\"card-icon-title\">Expenses</p>\n                        </span>\n                        \n                        <font-awesome-icon class=\"icon card-icon ms-auto\" :icon=\"['fa', 'times-circle']\" /> \n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <!-- Graphs -->\n    <div class=\"row mt-4\">\n        <!-- new clients bar graph -->\n        <div class=\"col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12\">\n            <div class=\"card stats bg-light-grey mb-4\">\n                <div class=\"card-body\">\n                    <h6 class=\"mt-4 mb-0 text-dark fw-bold\">New Clients</h6>\n                    <small class=\"text-muted mt-0\" style=\"margin-bottom: 1.5rem\">Acquired Clients Across 2020</small>\n                    <canvas id=\"subscribers\"></canvas>\n                </div>\n            </div>\n        </div>\n\n        <!-- revenues in line graph -->\n        <div class=\"col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12\">\n            <div class=\"card stats bg-light-grey mb-4\">\n                <div class=\"card-body\">\n                    <h6 class=\"mt-4 mb-0 text-dark fw-bold\">Revenues</h6>\n                    <small class=\"text-muted mt-0\" style=\"margin-bottom: 1.5rem\">Revenues across 2020</small>\n                    <canvas id=\"revenues\"></canvas>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12\">\n            <div class=\"card stats bg-light-grey mb-4\">\n                <div class=\"card-body\">\n                    <h6 class=\"mt-4 mb-0 text-dark fw-bold\">Revenues from different Locations</h6>\n                    <small class=\"text-muted mt-0\" style=\"margin-bottom: 1.5rem\">See division of revenues by locations.</small>\n                    <canvas id=\"locations\"></canvas>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12\">\n            <div class=\"card stats bg-light-grey mb-4\">\n                <div class=\"card-body\">\n                    <h6 class=\"mt-4 mb-0 text-dark fw-bold\">Subscribers by Gender</h6>\n                    <small class=\"text-muted mt-0\" style=\"margin-bottom: 1.5rem\">See division of subscribers by gender.</small>\n                    <canvas id=\"gender\"></canvas>\n                </div>\n            </div>\n        </div>\n\n        <!-- new renewals by timeline -->\n        <div class=\"col-xxl-8 col-xl-8 col-lg-6 col-md-12 col-sm-12\">\n            <div class=\"card stats bg-light-grey mb-4\">\n                <div class=\"card-body\">\n                    <h6 class=\"mt-4 mb-0 text-dark fw-bold\">Top Clients</h6>\n                    <small class=\"text-muted mt-0\" style=\"margin-bottom: 1.5rem\">Clients who brought in most revenue</small>\n                    <canvas id=\"clients\"></canvas>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"col-xxl-4 col-xl-4 col-lg-6 col-md-12 col-sm-12\">\n            <div class=\"card stats bg-light-grey mb-4\">\n                <div class=\"card-body\">\n                    <h6 class=\"mt-4 mb-0 text-dark fw-bold\">New Stuff</h6>\n                    <small class=\"text-muted mt-0\" style=\"margin-bottom: 1.5rem\">Annual Stuff by Time</small>\n                    <table class=\"table table-striped mt-4\">\n                        <thead class=\"thead-dark\">\n                            <th>#</th>\n                            <th>Stuff</th>\n                            <th>Ideas</th>\n                        </thead>\n\n                        <tbody>\n                            <tr>\n                                <td>1</td>\n                                <td>Stuff 1</td>\n                                <td>Sell to everyone</td>\n                            </tr>\n                            <tr>\n                                <td>2</td>\n                                <td>Stuff 2</td>\n                                <td>Sell to some people</td>\n                            </tr>\n\n                            <tr>\n                                <td>3</td>\n                                <td>Stuff 3</td>\n                                <td>Sell to little people</td>\n                            </tr>\n\n                            <tr>\n                                <td>4</td>\n                                <td>Stuff 4</td>\n                                <td>Sell later</td>\n                            </tr>\n\n                            <tr>\n                                <td>5</td>\n                                <td>Stuff 5</td>\n                                <td>Do not sell</td>\n                            </tr>\n                        </tbody>\n                    </table>\n\n                    <button class=\"btn btn-primary btn-sm rounded-pill\">\n                        <span class=\"ms-1 me-1\">\n                            View more <font-awesome-icon class=\"icon\" :icon=\"['fa', 'plus']\" /> \n                        </span>\n                    </button>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style lang=\"scss\" scoped>\n  @import '../../assets/scss/layout/_stats.scss';\n\n</style>\n\n<script>\nimport Chart from 'chart.js';\n\nexport default {\n    name: 'SalesDashboard',\n    mounted() {\n      this.subscribers()\n      this.revenues()\n      this.locations()\n      this.gender()\n      this.clients()\n    },\n    methods: {\n      formatNumber(num) {\n        return num.toString().replace(/(\\d)(?=(\\d{3})+(?!\\d))/g, '$1,')\n      },\n      subscribers() {\n        var ctx = document.getElementById('subscribers').getContext('2d');\n        var data = {\n            labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', \n            'September', 'October', 'November', 'December'],\n            datasets: [{\n                label: 'New Clients',\n                data: [52, 60, 90, 110, 139, 80, 92, 100, 220, 180, 180, 103],\n                backgroundColor: [\n                    'rgba(255, 99, 132, 0.6)',\n                    'rgba(54, 162, 235, 0.6)',\n                    'rgba(255, 206, 86, 0.6)',\n                    'rgba(75, 192, 192, 0.6)',\n                    'rgba(153, 102, 255, 0.6)',\n                    'rgba(255, 159, 64, 0.6)',\n                    'rgba(255, 99, 132, 0.6)',\n                    'rgba(54, 162, 235, 0.6)',\n                    'rgba(255, 206, 86, 0.6)',\n                    'rgba(75, 192, 192, 0.6)',\n                    'rgba(153, 102, 255, 0.6)',\n                    'rgba(255, 159, 64, 0.6)'\n                ],\n                borderColor: [\n                    'rgba(255, 99, 132, 0.6)',\n                    'rgba(54, 162, 235, 0.6)',\n                    'rgba(255, 206, 86, 0.6)',\n                    'rgba(75, 192, 192, 0.6)',\n                    'rgba(153, 102, 255, 0.6)',\n                    'rgba(255, 159, 64, 0.6)',\n                    'rgba(255, 99, 132, 0.6)',\n                    'rgba(54, 162, 235, 0.6)',\n                    'rgba(255, 206, 86, 0.6)',\n                    'rgba(75, 192, 192, 0.6)',\n                    'rgba(153, 102, 255, 0.6)',\n                    'rgba(255, 159, 64, 0.6)'\n                ],\n                borderWidth: 1\n            }]\n        }\n\n        var options =  {\n            scales: {\n                yAxes: [{\n                    ticks: {\n 