ent.querySelector('#sidebar-wrapper').classList.add('toggled');\n        document.querySelector('.backdrop').classList.add('on');\n      },\n      logout() {\n        localStorage.removeItem('user')\n        this.$router.push({name: 'Login'})\n      }\n    }\n}\n</script>"]}]}